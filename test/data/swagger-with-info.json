        "username_byte": {
          "type": "string",
          "format": "byte"
        },
        "username_binary": {
          "type": "string",
          "format": "binary"
        },
        "username_boolean": {
          "type": "boolean"
        },
        "username_date": {
          "type": "string",
          "format": "date"
        },
        "username_datetime": {
          "type": "string",
          "format": "date-time"
        },
        "user_password": {
          "type": "string",
          "format": "password"
