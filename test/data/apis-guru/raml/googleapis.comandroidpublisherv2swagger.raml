#%RAML 1.0
title: Google Play Developer
version: v2
baseUri: 'https://www.googleapis.com/androidpublisher/v2/applications'
protocols:
  - HTTPS
description: Lets Android application developers access their Google Play accounts.
(oas-tags-definition):
  - name: edits
  - name: entitlements
  - name: inappproducts
  - name: purchases
  - name: reviews
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-apiClientRegistration: any
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-apiClientRegistration):
    url: 'https://console.developers.google.com'
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_www.gstatic.com_android_market_images_web_play_logo_x2.png'
  (oas-x-origin):
    format: google
    url: 'https://www.googleapis.com/discovery/v1/apis/androidpublisher/v2/rest'
    version: v1
  (oas-x-preferred): true
  (oas-x-providerName): googleapis.com
  (oas-x-serviceName): androidpublisher
  contact:
    name: Google
    url: 'https://google.com'
(oas-externalDocs):
  url: 'https://developers.google.com/android-publisher'
securitySchemes:
  Oauth2:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.google.com/o/oauth2/auth'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - 'https://www.googleapis.com/auth/androidpublisher'
    description: Oauth 2.0 authentication
'/{packageName}':
  displayName: '{packageName}'
  /edits:
    displayName: edits
    '/{editId}':
      displayName: '{editId}'
      /listings:
        displayName: listings
        '/{language}':
          displayName: '{language}'
          '/{imageType}':
            displayName: '{imageType}'
            uriParameters:
              imageType:
                enum:
                  - featureGraphic
                  - icon
                  - phoneScreenshots
                  - promoGraphic
                  - sevenInchScreenshots
                  - tenInchScreenshots
                  - tvBanner
                  - tvScreenshots
                  - wearScreenshots
                required: true
                type: string
            get:
              displayName: androidpublisher.edits.images.list
              description: Lists all images for the specified language and image type.
              responses:
                '200':
                  body:
                    application/json:
                      type: ImagesListResponse
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/androidpublisher'
              (oas-tags):
                - edits
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
            post:
              displayName: androidpublisher.edits.images.upload
              description: Uploads a new image and adds it to the list of images for the specified language and image type.
              responses:
                '200':
                  body:
                    application/json:
                      type: ImagesUploadResponse
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/androidpublisher'
              (oas-tags):
                - edits
            '/{imageId}':
              displayName: '{imageId}'
              uriParameters:
                imageId:
                  description: Unique identifier an image within the set of images attached to this edit.
                  required: true
                  type: string
              delete:
                displayName: androidpublisher.edits.images.delete
                description: Deletes the image (specified by id) from the edit.
                responses:
                  '200':
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              is:
                - alt
                - fields
                - key
                - oauthToken
                - prettyPrint
                - quotaUser
                - userIp
            delete:
              displayName: androidpublisher.edits.images.deleteall
              description: Deletes all images for the specified language and image type.
              responses:
                '200':
                  body:
                    application/json:
                      type: ImagesDeleteAllResponse
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/androidpublisher'
              (oas-tags):
                - edits
          uriParameters:
            language:
              description: 'The language code (a BCP-47 language tag) of the localized listing to read or modify. For example, to select Austrian German, pass "de-AT".'
              required: true
              type: string
          get:
            displayName: androidpublisher.edits.listings.get
            description: Fetches information about a localized store listing.
            responses:
              '200':
                body:
                  application/json:
                    type: Listing
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
          put:
            displayName: androidpublisher.edits.listings.update
            description: Creates or updates a localized store listing.
            body:
              application/json:
                type: Listing
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: Listing
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          patch:
            displayName: androidpublisher.edits.listings.patch
            description: Creates or updates a localized store listing. This method supports patch semantics.
            body:
              application/json:
                type: Listing
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: Listing
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          delete:
            displayName: androidpublisher.edits.listings.delete
            description: Deletes the specified localized store listing from an edit.
            responses:
              '200':
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
        get:
          displayName: androidpublisher.edits.listings.list
          description: Returns all of the localized store listings attached to this edit.
          responses:
            '200':
              body:
                application/json:
                  type: ListingsListResponse
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        delete:
          displayName: androidpublisher.edits.listings.deleteall
          description: Deletes all localized listings from an edit.
          responses:
            '200':
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
      uriParameters:
        editId:
          description: Unique identifier for this edit.
          required: true
          type: string
      delete:
        displayName: androidpublisher.edits.delete
        description: Deletes an edit for an app. Creating a new edit will automatically delete any of your previous edits so this method need only be called if you want to preemptively abandon an edit.
        responses:
          '200':
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/androidpublisher'
        (oas-tags):
          - edits
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      /testers:
        displayName: testers
        '/{track}':
          displayName: '{track}'
          uriParameters:
            track:
              enum:
                - alpha
                - beta
                - production
                - rollout
              required: true
              type: string
          get:
            displayName: androidpublisher.edits.testers.get
            responses:
              '200':
                body:
                  application/json:
                    type: Testers
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
          patch:
            displayName: androidpublisher.edits.testers.patch
            body:
              application/json:
                type: Testers
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: Testers
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          put:
            displayName: androidpublisher.edits.testers.update
            body:
              application/json:
                type: Testers
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: Testers
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
      /apks:
        displayName: apks
        get:
          displayName: androidpublisher.edits.apks.list
          responses:
            '200':
              body:
                application/json:
                  type: ApksListResponse
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        /externallyHosted:
          displayName: externallyHosted
          post:
            displayName: androidpublisher.edits.apks.addexternallyhosted
            description: 'Creates a new APK without uploading the APK itself to Google Play, instead hosting the APK at a specified URL. This function is only available to enterprises using Google Play for Work whose application is configured to restrict distribution to the enterprise domain.'
            body:
              application/json:
                type: ApksAddExternallyHostedRequest
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: ApksAddExternallyHostedResponse
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
        '/{apkVersionCode}':
          displayName: '{apkVersionCode}'
          /expansionFiles:
            displayName: expansionFiles
            '/{expansionFileType}':
              displayName: '{expansionFileType}'
              uriParameters:
                expansionFileType:
                  enum:
                    - main
                    - patch
                  required: true
                  type: string
              get:
                displayName: androidpublisher.edits.expansionfiles.get
                description: Fetches the Expansion File configuration for the APK specified.
                responses:
                  '200':
                    body:
                      application/json:
                        type: ExpansionFile
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              is:
                - alt
                - fields
                - key
                - oauthToken
                - prettyPrint
                - quotaUser
                - userIp
              post:
                displayName: androidpublisher.edits.expansionfiles.upload
                description: Uploads and attaches a new Expansion File to the APK specified.
                responses:
                  '200':
                    body:
                      application/json:
                        type: ExpansionFilesUploadResponse
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              patch:
                displayName: androidpublisher.edits.expansionfiles.patch
                description: Updates the APK's Expansion File configuration to reference another APK's Expansion Files. To add a new Expansion File use the Upload method. This method supports patch semantics.
                body:
                  application/json:
                    type: ExpansionFile
                    (oas-body-name): body
                responses:
                  '200':
                    body:
                      application/json:
                        type: ExpansionFile
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              put:
                displayName: androidpublisher.edits.expansionfiles.update
                description: Updates the APK's Expansion File configuration to reference another APK's Expansion Files. To add a new Expansion File use the Upload method.
                body:
                  application/json:
                    type: ExpansionFile
                    (oas-body-name): body
                responses:
                  '200':
                    body:
                      application/json:
                        type: ExpansionFile
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
          /listings:
            displayName: listings
            delete:
              displayName: androidpublisher.edits.apklistings.deleteall
              description: Deletes all the APK-specific localized listings for a specified APK.
              responses:
                '200':
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/androidpublisher'
              (oas-tags):
                - edits
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
            '/{language}':
              displayName: '{language}'
              uriParameters:
                language:
                  description: 'The language code (a BCP-47 language tag) of the APK-specific localized listing to read or modify. For example, to select Austrian German, pass "de-AT".'
                  required: true
                  type: string
              delete:
                displayName: androidpublisher.edits.apklistings.delete
                description: Deletes the APK-specific localized listing for a specified APK and language code.
                responses:
                  '200':
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              is:
                - alt
                - fields
                - key
                - oauthToken
                - prettyPrint
                - quotaUser
                - userIp
              patch:
                displayName: androidpublisher.edits.apklistings.patch
                description: Updates or creates the APK-specific localized listing for a specified APK and language code. This method supports patch semantics.
                body:
                  application/json:
                    type: ApkListing
                    (oas-body-name): body
                responses:
                  '200':
                    body:
                      application/json:
                        type: ApkListing
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              put:
                displayName: androidpublisher.edits.apklistings.update
                description: Updates or creates the APK-specific localized listing for a specified APK and language code.
                body:
                  application/json:
                    type: ApkListing
                    (oas-body-name): body
                responses:
                  '200':
                    body:
                      application/json:
                        type: ApkListing
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              get:
                displayName: androidpublisher.edits.apklistings.get
                description: Fetches the APK-specific localized listing for a specified APK and language code.
                responses:
                  '200':
                    body:
                      application/json:
                        type: ApkListing
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
            get:
              displayName: androidpublisher.edits.apklistings.list
              description: Lists all the APK-specific localized listings for a specified APK.
              responses:
                '200':
                  body:
                    application/json:
                      type: ApkListingsListResponse
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/androidpublisher'
              (oas-tags):
                - edits
          /deobfuscationFiles:
            displayName: deobfuscationFiles
            '/{deobfuscationFileType}':
              displayName: '{deobfuscationFileType}'
              uriParameters:
                deobfuscationFileType:
                  enum:
                    - proguard
                  required: true
                  type: string
              post:
                displayName: androidpublisher.edits.deobfuscationfiles.upload
                description: 'Uploads the deobfuscation file of the specified APK. If a deobfuscation file already exists, it will be replaced.'
                responses:
                  '200':
                    body:
                      application/json:
                        type: DeobfuscationFilesUploadResponse
                    description: Successful response
                securedBy:
                  - Oauth2:
                      scopes:
                        - 'https://www.googleapis.com/auth/androidpublisher'
                (oas-tags):
                  - edits
              is:
                - alt
                - fields
                - key
                - oauthToken
                - prettyPrint
                - quotaUser
                - userIp
        post:
          displayName: androidpublisher.edits.apks.upload
          responses:
            '200':
              body:
                application/json:
                  type: Apk
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
      /tracks:
        displayName: tracks
        get:
          displayName: androidpublisher.edits.tracks.list
          description: Lists all the track configurations for this edit.
          responses:
            '200':
              body:
                application/json:
                  type: TracksListResponse
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        '/{track}':
          displayName: '{track}'
          uriParameters:
            track:
              description: The track type to read or modify.
              enum:
                - alpha
                - beta
                - production
                - rollout
              required: true
              type: string
          get:
            displayName: androidpublisher.edits.tracks.get
            description: Fetches the track configuration for the specified track type. Includes the APK version codes that are in this track.
            responses:
              '200':
                body:
                  application/json:
                    type: Track
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
          patch:
            displayName: androidpublisher.edits.tracks.patch
            description: 'Updates the track configuration for the specified track type. When halted, the rollout track cannot be updated without adding new APKs, and adding new APKs will cause it to resume. This method supports patch semantics.'
            body:
              application/json:
                type: Track
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: Track
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
          put:
            displayName: androidpublisher.edits.tracks.update
            description: 'Updates the track configuration for the specified track type. When halted, the rollout track cannot be updated without adding new APKs, and adding new APKs will cause it to resume.'
            body:
              application/json:
                type: Track
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: Track
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/androidpublisher'
            (oas-tags):
              - edits
      /details:
        displayName: details
        get:
          displayName: androidpublisher.edits.details.get
          description: Fetches app details for this edit. This includes the default language and developer support contact information.
          responses:
            '200':
              body:
                application/json:
                  type: AppDetails
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        put:
          displayName: androidpublisher.edits.details.update
          description: Updates app details for this edit.
          body:
            application/json:
              type: AppDetails
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: AppDetails
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
        patch:
          displayName: androidpublisher.edits.details.patch
          description: Updates app details for this edit. This method supports patch semantics.
          body:
            application/json:
              type: AppDetails
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: AppDetails
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/androidpublisher'
          (oas-tags):
            - edits
      get:
        displayName: androidpublisher.edits.get
        description: 'Returns information about the edit specified. Calls will fail if the edit is no long active (e.g. has been deleted, superseded or expired).'
        responses:
          '200':
            body:
              application/json:
                type: AppEdit
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/androidpublisher'
        (oas-tags):
          - edits
    '/{editId}:commit':
      displayName: '{editId}:commit'
      uriParameters:
        editId:
          description: Unique identifier for this edit.
          required: true
          type: string
      post:
        displayName: androidpublisher.edits.commit
        description: Commits/applies the changes made in this edit back to the app.
        responses:
          '200':
            body:
              application/json:
                type: AppEdit
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/androidpublisher'
        (oas-tags):
          - edits
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    '/{editId}:validate':
      displayName: '{editId}:validate'
      uriParameters:
        editId:
          description: Unique identifier for this edit.
          required: true
          type: string
      post:
        displayName: androidpublisher.edits.validate
        description: Checks that the edit can be successfully committed. The edit's changes are not applied to the live app.
        responses:
          '200':
            body:
              application/json:
                type: AppEdit
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/androidpublisher'
        (oas-tags):
          - edits
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    post:
      displayName: androidpublisher.edits.insert
      description: 'Creates a new edit for an app, populated with the app''s current state.'
      body:
