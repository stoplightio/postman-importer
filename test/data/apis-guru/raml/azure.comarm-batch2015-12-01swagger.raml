                          displayName: ApplicationPackage_Get
                          description: Gets information about the specified application package.
                          is:
                            - apiVersionParameter
                          (oas-responses-default):
                            body:
                              application/json:
                                type: ErrorBody
                            description: Error response describing why the operation failed.
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: ApplicationPackage
                              description: The operation was successful. The response contains the application package entity.
                          (oas-tags):
                            - ApplicationPackage
                        /activate:
                          displayName: activate
                          post:
                            displayName: ApplicationPackage_Activate
                            description: Activates the specified application package.
                            is:
                              - apiVersionParameter
                            body:
                              application/json:
                                type: ActivateApplicationPackageParameters
                                (oas-body-name): parameters
                                description: The parameters for the request.
                            (oas-responses-default):
                              body:
                                application/json:
                                  type: ErrorBody
                              description: Error response describing why the operation failed.
                            responses:
                              '204':
                                description: The operation was successful.
                            (oas-tags):
                              - ApplicationPackage
                        put:
                          displayName: ApplicationPackage_Create
                          description: Creates an application package record.
                          is:
                            - apiVersionParameter
                          (oas-responses-default):
                            body:
                              application/json:
                                type: ErrorBody
                            description: Error response describing why the operation failed.
                          responses:
                            '201':
                              body:
                                application/json:
                                  type: ApplicationPackage
                              description: The operation was successful. The response contains the application package entity.
