              '/{accountName}':
                displayName: '{accountName}'
                /applications:
                  displayName: applications
                  '/{applicationId}':
                    displayName: '{applicationId}'
                    uriParameters:
                      applicationId:
                        description: The ID of the application.
                        required: true
                        type: string
                        displayName: The ID of the application.
                    get:
                      displayName: Application_Get
                      description: Gets information about the specified application.
                      is:
                        - apiVersionParameter
