                      maxresults:
                        description: The maximum number of items to return in the response.
                        format: int32
                        required: false
                        type: integer
                        displayName: The maximum number of items to return in the response.
                    (oas-tags):
                      - Application
                /syncAutoStorageKeys:
                  displayName: syncAutoStorageKeys
