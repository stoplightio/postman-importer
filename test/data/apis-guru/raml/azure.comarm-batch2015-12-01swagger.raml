                      (oas-responses-default):
                        body:
                          application/json:
                            type: ErrorBody
                        description: Error response describing why the operation failed.
                      responses:
                        '201':
                          body:
                            application/json:
                              type: Application
                          description: The operation was successful. The response contains the application entity.
                      (oas-tags):
                        - Application
                    /versions:
                      displayName: versions
