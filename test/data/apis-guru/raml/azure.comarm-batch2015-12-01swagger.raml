                      - apiVersionParameter
                    (oas-responses-default):
                      body:
                        application/json:
                          type: ErrorBody
                      description: Error response describing why the operation failed.
                    responses:
                      '204':
                        description: The operation was successful.
                    (oas-tags):
                      - BatchAccount
                uriParameters:
                  accountName:
                    description: The name of the account to be deleted.
                    maxLength: 24
                    minLength: 3
                    pattern: '^[-\w\._]+$'
                    required: true
                    type: string
                    displayName: The name of the account to be deleted.
                get:
                  displayName: BatchAccount_Get
                  description: Gets information about the specified Batch account.
                  is:
                    - apiVersionParameter
                  (oas-responses-default):
                    body:
                      application/json:
                        type: ErrorBody
                    description: Error response describing why the operation failed.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: BatchAccount
                      description: The operation was successful. The response contains the Batch account entity.
                  (oas-tags):
                    - BatchAccount
                put:
                  (oas-x-ms-long-running-operation): true
