                          application/json:
                            type: ErrorBody
                        description: Error response describing why the operation failed.
                      responses:
                        '204':
                          description: The operation was successful.
                      (oas-tags):
                        - Application
                  get:
                    (oas-x-ms-pageable):
                      nextLinkName: nextLink
                    displayName: Application_List
                    description: Lists all of the applications in the specified account.
                    is:
                      - apiVersionParameter
                    (oas-responses-default):
                      body:
                        application/json:
                          type: ErrorBody
                      description: Error response describing why the operation failed.
                    responses:
                      '200':
                        body:
                          application/json:
                            type: ListApplicationsResult
                        description: The operation was successful. The response contains a list of the application entities associated with the specified account.
