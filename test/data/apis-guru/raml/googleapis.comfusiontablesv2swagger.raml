    - oauthToken
    - prettyPrint
    - quotaUser
    - userIp
  /import:
    displayName: import
    post:
      displayName: fusiontables.table.importTable
      description: Imports a new table.
      responses:
        '200':
          body:
            application/json:
              type: Table
          description: Successful response
      queryParameters:
        name:
          description: The name to be assigned to the new table.
          required: true
          type: string
          displayName: The name to be assigned to the new table.
        delimiter:
          description: 'The delimiter used to separate cell values. This can only consist of a single character. Default is ,.'
          type: string
          displayName: 'The delimiter used to separate cell values. This can only consist of a single character. Default is ,.'
        encoding:
          description: The encoding of the content. Default is UTF-8. Use auto-detect if you are unsure of the encoding.
