#%RAML 1.0
title: Fusion Tables
version: v2
baseUri: 'https://www.googleapis.com/fusiontables/v2'
protocols:
  - HTTPS
description: API for working with Fusion Tables data.
(oas-tags-definition):
  - name: column
  - name: query
  - name: style
  - name: table
  - name: task
  - name: template
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-apiClientRegistration: any
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-apiClientRegistration):
    url: 'https://console.developers.google.com'
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_www.google.com_images_branding_googlelogo_2x_googlelogo_color_272x92dp.png'
  (oas-x-origin):
    format: google
    url: 'https://www.googleapis.com/discovery/v1/apis/fusiontables/v2/rest'
    version: v1
  (oas-x-preferred): true
  (oas-x-providerName): googleapis.com
  (oas-x-serviceName): fusiontables
  contact:
    name: Google
    url: 'https://google.com'
(oas-externalDocs):
  url: 'https://developers.google.com/fusiontables'
securitySchemes:
  Oauth2:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.google.com/o/oauth2/auth'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - 'https://www.googleapis.com/auth/fusiontables'
        - 'https://www.googleapis.com/auth/fusiontables.readonly'
    description: Oauth 2.0 authentication
/tables:
  displayName: tables
  '/{tableId}':
    displayName: '{tableId}'
    /import:
      displayName: import
      post:
        displayName: fusiontables.table.importRows
        description: Imports more rows into a table.
        responses:
          '200':
            body:
              application/json:
                type: Import
            description: Successful response
        queryParameters:
          delimiter:
            description: 'The delimiter used to separate cell values. This can only consist of a single character. Default is ,.'
            type: string
            displayName: 'The delimiter used to separate cell values. This can only consist of a single character. Default is ,.'
          encoding:
            description: The encoding of the content. Default is UTF-8. Use auto-detect if you are unsure of the encoding.
            type: string
            displayName: The encoding of the content. Default is UTF-8. Use auto-detect if you are unsure of the encoding.
          endLine:
            description: 'The index of the line up to which data will be imported. Default is to import the entire file. If endLine is negative, it is an offset from the end of the file; the imported content will exclude the last endLine lines.'
            type: integer
            displayName: 'The index of the line up to which data will be imported. Default is to import the entire file. If endLine is negative, it is an offset from the end of the file; the imported content will exclude the last endLine lines.'
          isStrict:
            description: 'Whether the imported CSV must have the same number of values for each row. If false, rows with fewer values will be padded with empty values. Default is true.'
            type: boolean
            displayName: 'Whether the imported CSV must have the same number of values for each row. If false, rows with fewer values will be padded with empty values. Default is true.'
          startLine:
            description: 'The index of the first line from which to start importing, inclusive. Default is 0.'
            type: integer
            displayName: 'The index of the first line from which to start importing, inclusive. Default is 0.'
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/fusiontables'
        (oas-tags):
          - table
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /templates:
      displayName: templates
      '/{templateId}':
        displayName: '{templateId}'
        uriParameters:
          templateId:
            description: Identifier for the template which is being deleted
            required: true
            type: integer
            displayName: Identifier for the template which is being deleted
        put:
          displayName: fusiontables.template.update
          description: Updates an existing template
          body:
            application/json:
              type: Template
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Template
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables'
          (oas-tags):
            - template
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        patch:
          displayName: fusiontables.template.patch
          description: Updates an existing template. This method supports patch semantics.
          body:
            application/json:
              type: Template
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Template
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables'
          (oas-tags):
            - template
        get:
          displayName: fusiontables.template.get
          description: Retrieves a specific template by its id
          responses:
            '200':
              body:
                application/json:
                  type: Template
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables.readonly'
          (oas-tags):
            - template
        delete:
          displayName: fusiontables.template.delete
          description: Deletes a template
          responses:
            '200':
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables'
          (oas-tags):
            - template
      get:
        displayName: fusiontables.template.list
        description: Retrieves a list of templates.
        responses:
          '200':
            body:
              application/json:
                type: TemplateList
            description: Successful response
        queryParameters:
          maxResults:
            description: Maximum number of templates to return. Optional. Default is 5.
            minimum: 0
            type: integer
            displayName: Maximum number of templates to return. Optional. Default is 5.
          pageToken:
            description: Continuation token specifying which results page to return. Optional.
            type: string
            displayName: Continuation token specifying which results page to return. Optional.
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/fusiontables.readonly'
        (oas-tags):
          - template
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      post:
        displayName: fusiontables.template.insert
        description: Creates a new template for the table.
        body:
          application/json:
            type: Template
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: Template
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/fusiontables'
        (oas-tags):
          - template
    /replace:
      displayName: replace
      post:
        displayName: fusiontables.table.replaceRows
        description: Replaces rows of an existing table. Current rows remain visible until all replacement rows are ready.
        responses:
          '200':
            body:
              application/json:
                type: Task
            description: Successful response
        queryParameters:
          delimiter:
            description: 'The delimiter used to separate cell values. This can only consist of a single character. Default is ,.'
            type: string
            displayName: 'The delimiter used to separate cell values. This can only consist of a single character. Default is ,.'
          encoding:
            description: The encoding of the content. Default is UTF-8. Use 'auto-detect' if you are unsure of the encoding.
            type: string
            displayName: The encoding of the content. Default is UTF-8. Use 'auto-detect' if you are unsure of the encoding.
          endLine:
            description: 'The index of the line up to which data will be imported. Default is to import the entire file. If endLine is negative, it is an offset from the end of the file; the imported content will exclude the last endLine lines.'
            type: integer
            displayName: 'The index of the line up to which data will be imported. Default is to import the entire file. If endLine is negative, it is an offset from the end of the file; the imported content will exclude the last endLine lines.'
          isStrict:
            description: 'Whether the imported CSV must have the same number of column values for each row. If true, throws an exception if the CSV does not have the same number of columns. If false, rows with fewer column values will be padded with empty values. Default is true.'
            type: boolean
            displayName: 'Whether the imported CSV must have the same number of column values for each row. If true, throws an exception if the CSV does not have the same number of columns. If false, rows with fewer column values will be padded with empty values. Default is true.'
          startLine:
            description: 'The index of the first line from which to start importing, inclusive. Default is 0.'
            type: integer
            displayName: 'The index of the first line from which to start importing, inclusive. Default is 0.'
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/fusiontables'
        (oas-tags):
          - table
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /styles:
      displayName: styles
      get:
