/query:
  displayName: query
  get:
    displayName: fusiontables.query.sqlGet
    description: |-
      Executes a SQL statement which can be any of 
      - SELECT
      - SHOW
      - DESCRIBE
    responses:
      '200':
        body:
          application/json:
            type: Sqlresponse
        description: Successful response
    queryParameters:
      sql:
        description: |-
          A SQL statement which can be any of 
          - SELECT
          - SHOW
          - DESCRIBE
        required: true
        type: string
        displayName: |-
          A SQL statement which can be any of 
          - SELECT
          - SHOW
          - DESCRIBE
      hdrs:
        description: Whether column names are included (in the first row). Default is true.
        type: boolean
