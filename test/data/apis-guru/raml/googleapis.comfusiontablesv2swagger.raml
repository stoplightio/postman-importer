        description: Retrieves a list of styles.
        responses:
          '200':
            body:
              application/json:
                type: StyleSettingList
            description: Successful response
        queryParameters:
          maxResults:
            description: Maximum number of styles to return. Optional. Default is 5.
            minimum: 0
            type: integer
            displayName: Maximum number of styles to return. Optional. Default is 5.
          pageToken:
            description: Continuation token specifying which result page to return. Optional.
            type: string
            displayName: Continuation token specifying which result page to return. Optional.
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/fusiontables.readonly'
        (oas-tags):
          - style
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      post:
        displayName: fusiontables.style.insert
        description: Adds a new style for the table.
        body:
          application/json:
            type: StyleSetting
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: StyleSetting
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/fusiontables'
        (oas-tags):
          - style
      '/{styleId}':
        displayName: '{styleId}'
        uriParameters:
          styleId:
            description: Identifier (within a table) for the style being updated.
            required: true
            type: integer
            displayName: Identifier (within a table) for the style being updated.
        delete:
          displayName: fusiontables.style.delete
          description: Deletes a style.
          responses:
            '200':
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables'
          (oas-tags):
            - style
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        get:
          displayName: fusiontables.style.get
          description: Gets a specific style.
          responses:
            '200':
              body:
                application/json:
                  type: StyleSetting
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables.readonly'
          (oas-tags):
            - style
        patch:
          displayName: fusiontables.style.patch
          description: Updates an existing style. This method supports patch semantics.
          body:
            application/json:
              type: StyleSetting
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: StyleSetting
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables'
          (oas-tags):
            - style
        put:
          displayName: fusiontables.style.update
          description: Updates an existing style.
          body:
            application/json:
              type: StyleSetting
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: StyleSetting
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables'
          (oas-tags):
            - style
    uriParameters:
      tableId:
        description: ID of the table that is being updated.
        required: true
        type: string
        displayName: ID of the table that is being updated.
    get:
      displayName: fusiontables.table.get
      description: Retrieves a specific table by its ID.
      responses:
        '200':
          body:
            application/json:
              type: Table
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/fusiontables.readonly'
      (oas-tags):
        - table
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    put:
      displayName: fusiontables.table.update
      description: 'Updates an existing table. Unless explicitly requested, only the name, description, and attribution will be updated.'
      body:
        application/json:
          type: Table
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: Table
          description: Successful response
      queryParameters:
        replaceViewDefinition:
          description: Whether the view definition is also updated. The specified view definition replaces the existing one. Only a view can be updated with a new definition.
          type: boolean
          displayName: Whether the view definition is also updated. The specified view definition replaces the existing one. Only a view can be updated with a new definition.
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/fusiontables'
      (oas-tags):
