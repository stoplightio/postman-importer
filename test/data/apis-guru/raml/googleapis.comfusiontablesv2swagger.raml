          displayName: fusiontables.column.update
          description: Updates the name or type of an existing column.
          body:
            application/json:
              type: Column
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Column
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/fusiontables'
          (oas-tags):
            - column
        patch:
          displayName: fusiontables.column.patch
          description: Updates the name or type of an existing column. This method supports patch semantics.
          body:
            application/json:
              type: Column
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
