          application/json:
            type: TableList
        description: Successful response
    queryParameters:
      maxResults:
        description: Maximum number of tables to return. Default is 5.
        minimum: 0
        type: integer
        displayName: Maximum number of tables to return. Default is 5.
      pageToken:
