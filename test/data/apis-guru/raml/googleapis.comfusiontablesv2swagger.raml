      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      '/{columnId}':
        displayName: '{columnId}'
        uriParameters:
          columnId:
            description: Name or identifier for the column being deleted.
            required: true
            type: string
            displayName: Name or identifier for the column being deleted.
        get:
          displayName: fusiontables.column.get
          description: Retrieves a specific column by its ID.
          responses:
            '200':
              body:
                application/json:
                  type: Column
              description: Successful response
          securedBy:
            - Oauth2:
