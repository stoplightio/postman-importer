#%RAML 1.0
title: Drive
version: v2
baseUri: 'https://www.googleapis.com/drive/v2'
protocols:
  - HTTPS
description: 'Manages files in Drive including uploading, downloading, searching, detecting changes, and updating sharing permissions.'
(oas-tags-definition):
  - name: about
  - name: apps
  - name: changes
  - name: channels
  - name: children
  - name: comments
  - name: files
  - name: parents
  - name: permissions
  - name: properties
  - name: realtime
  - name: replies
  - name: revisions
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-apiClientRegistration: any
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-apiClientRegistration):
    url: 'https://console.developers.google.com'
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_www.gstatic.com_images_icons_material_product_2x_drive_32dp.png'
  (oas-x-origin):
    format: google
    url: 'https://www.googleapis.com/discovery/v1/apis/drive/v2/rest'
    version: v1
  (oas-x-preferred): false
  (oas-x-providerName): googleapis.com
  (oas-x-serviceName): drive
  contact:
    name: Google
    url: 'https://google.com'
(oas-externalDocs):
  url: 'https://developers.google.com/drive/'
securitySchemes:
  Oauth2:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.google.com/o/oauth2/auth'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - 'https://www.googleapis.com/auth/drive'
        - 'https://www.googleapis.com/auth/drive.appdata'
        - 'https://www.googleapis.com/auth/drive.apps.readonly'
        - 'https://www.googleapis.com/auth/drive.file'
        - 'https://www.googleapis.com/auth/drive.metadata'
        - 'https://www.googleapis.com/auth/drive.metadata.readonly'
        - 'https://www.googleapis.com/auth/drive.photos.readonly'
        - 'https://www.googleapis.com/auth/drive.readonly'
        - 'https://www.googleapis.com/auth/drive.scripts'
    description: Oauth 2.0 authentication
/files:
  displayName: files
  '/{fileId}':
    displayName: '{fileId}'
    /parents:
      displayName: parents
      post:
        displayName: drive.parents.insert
        description: Adds a parent folder for a file.
        body:
          application/json:
            type: ParentReference
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: ParentReference
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/drive.file'
        (oas-tags):
          - parents
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      '/{parentId}':
        displayName: '{parentId}'
        uriParameters:
          parentId:
            description: The ID of the parent.
            required: true
            type: string
            displayName: The ID of the parent.
        delete:
          displayName: drive.parents.delete
          description: Removes a parent from a file.
          responses:
            '200':
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.file'
          (oas-tags):
            - parents
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        get:
          displayName: drive.parents.get
          description: Gets a specific parent reference.
          responses:
            '200':
              body:
                application/json:
                  type: ParentReference
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.readonly'
          (oas-tags):
            - parents
      get:
        displayName: drive.parents.list
        description: Lists a file's parents.
        responses:
          '200':
            body:
              application/json:
                type: ParentList
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/drive.readonly'
        (oas-tags):
          - parents
    /permissions:
      displayName: permissions
      get:
        displayName: drive.permissions.list
        description: Lists a file's permissions.
        responses:
          '200':
            body:
              application/json:
                type: PermissionList
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/drive.readonly'
        (oas-tags):
          - permissions
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      post:
        displayName: drive.permissions.insert
        description: Inserts a permission for a file.
        body:
          application/json:
            type: Permission
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: Permission
            description: Successful response
        queryParameters:
          emailMessage:
            description: A custom message to include in notification emails.
            type: string
            displayName: A custom message to include in notification emails.
          sendNotificationEmails:
            default: true
            description: Whether to send notification emails when sharing to users or groups. This parameter is ignored and an email is sent if the role is owner.
            type: boolean
            displayName: Whether to send notification emails when sharing to users or groups. This parameter is ignored and an email is sent if the role is owner.
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/drive.file'
        (oas-tags):
          - permissions
      '/{permissionId}':
        displayName: '{permissionId}'
        uriParameters:
          permissionId:
            description: The ID for the permission.
            required: true
            type: string
            displayName: The ID for the permission.
        delete:
          displayName: drive.permissions.delete
          description: Deletes a permission from a file.
          responses:
            '200':
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.file'
          (oas-tags):
            - permissions
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        get:
          displayName: drive.permissions.get
          description: Gets a permission by ID.
          responses:
            '200':
              body:
                application/json:
                  type: Permission
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.readonly'
          (oas-tags):
            - permissions
        patch:
          displayName: drive.permissions.patch
          description: Updates a permission using patch semantics.
          body:
            application/json:
              type: Permission
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Permission
              description: Successful response
          queryParameters:
            removeExpiration:
              default: false
              description: Whether to remove the expiration date.
              type: boolean
              displayName: Whether to remove the expiration date.
            transferOwnership:
              default: false
              description: Whether changing a role to 'owner' downgrades the current owners to writers. Does nothing if the specified role is not 'owner'.
              type: boolean
              displayName: Whether changing a role to 'owner' downgrades the current owners to writers. Does nothing if the specified role is not 'owner'.
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.file'
          (oas-tags):
            - permissions
        put:
          displayName: drive.permissions.update
          description: Updates a permission.
          body:
            application/json:
              type: Permission
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Permission
              description: Successful response
          queryParameters:
            removeExpiration:
              default: false
              description: Whether to remove the expiration date.
              type: boolean
              displayName: Whether to remove the expiration date.
            transferOwnership:
              default: false
              description: Whether changing a role to 'owner' downgrades the current owners to writers. Does nothing if the specified role is not 'owner'.
              type: boolean
              displayName: Whether changing a role to 'owner' downgrades the current owners to writers. Does nothing if the specified role is not 'owner'.
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.file'
          (oas-tags):
            - permissions
    uriParameters:
      fileId:
        description: The ID of the file to delete.
        required: true
        type: string
        displayName: The ID of the file to delete.
    get:
      displayName: drive.files.get
      description: Gets a file's metadata by ID.
      responses:
        '200':
          body:
            application/json:
              type: File
          description: Successful response
      queryParameters:
        acknowledgeAbuse:
          default: false
          description: Whether the user is acknowledging the risk of downloading known malware or other abusive files.
          type: boolean
          displayName: Whether the user is acknowledging the risk of downloading known malware or other abusive files.
        projection:
          description: This parameter is deprecated and has no function.
          enum:
            - BASIC
            - FULL
          type: string
          displayName: This parameter is deprecated and has no function.
        revisionId:
          description: Specifies the Revision ID that should be downloaded. Ignored unless alt=media is specified.
          type: string
          displayName: Specifies the Revision ID that should be downloaded. Ignored unless alt=media is specified.
        updateViewedDate:
          default: false
          description: 'Deprecated: Use files.update with modifiedDateBehavior=noChange, updateViewedDate=true and an empty request body.'
          type: boolean
          displayName: 'Deprecated: Use files.update with modifiedDateBehavior=noChange, updateViewedDate=true and an empty request body.'
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/drive.readonly'
      (oas-tags):
        - files
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    put:
      displayName: drive.files.update
      description: Updates file metadata and/or content.
      body:
        application/json:
          type: File
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: File
          description: Successful response
      queryParameters:
        addParents:
          description: Comma-separated list of parent IDs to add.
          type: string
          displayName: Comma-separated list of parent IDs to add.
        convert:
          default: false
          description: This parameter is deprecated and has no function.
          type: boolean
          displayName: This parameter is deprecated and has no function.
        modifiedDateBehavior:
          description: Determines the behavior in which modifiedDate is updated. This overrides setModifiedDate.
          enum:
            - fromBody
            - fromBodyIfNeeded
            - fromBodyOrNow
            - noChange
            - now
            - nowIfNeeded
          type: string
          displayName: Determines the behavior in which modifiedDate is updated. This overrides setModifiedDate.
        newRevision:
          default: true
          description: 'Whether a blob upload should create a new revision. If false, the blob data in the current head revision is replaced. If true or not set, a new blob is created as head revision, and previous unpinned revisions are preserved for a short period of time. Pinned revisions are stored indefinitely, using additional storage quota, up to a maximum of 200 revisions. For details on how revisions are retained, see the Drive Help Center.'
          type: boolean
          displayName: 'Whether a blob upload should create a new revision. If false, the blob data in the current head revision is replaced. If true or not set, a new blob is created as head revision, and previous unpinned revisions are preserved for a short period of time. Pinned revisions are stored indefinitely, using additional storage quota, up to a maximum of 200 revisions. For details on how revisions are retained, see the Drive Help Center.'
        ocr:
          default: false
          description: 'Whether to attempt OCR on .jpg, .png, .gif, or .pdf uploads.'
          type: boolean
          displayName: 'Whether to attempt OCR on .jpg, .png, .gif, or .pdf uploads.'
        ocrLanguage:
          description: 'If ocr is true, hints at the language to use. Valid values are BCP 47 codes.'
          type: string
          displayName: 'If ocr is true, hints at the language to use. Valid values are BCP 47 codes.'
        pinned:
          default: false
          description: Whether to pin the new revision. A file can have a maximum of 200 pinned revisions.
          type: boolean
          displayName: Whether to pin the new revision. A file can have a maximum of 200 pinned revisions.
        removeParents:
          description: Comma-separated list of parent IDs to remove.
          type: string
          displayName: Comma-separated list of parent IDs to remove.
        setModifiedDate:
          default: false
          description: Whether to set the modified date with the supplied modified date.
          type: boolean
          displayName: Whether to set the modified date with the supplied modified date.
        timedTextLanguage:
          description: The language of the timed text.
          type: string
          displayName: The language of the timed text.
        timedTextTrackName:
          description: The timed text track name.
          type: string
          displayName: The timed text track name.
        updateViewedDate:
          default: true
          description: Whether to update the view date after successfully updating the file.
          type: boolean
          displayName: Whether to update the view date after successfully updating the file.
        useContentAsIndexableText:
          default: false
          description: Whether to use the content as indexable text.
          type: boolean
          displayName: Whether to use the content as indexable text.
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/drive.scripts'
      (oas-tags):
        - files
    /properties:
      displayName: properties
      get:
        displayName: drive.properties.list
        description: Lists a file's properties.
        responses:
          '200':
            body:
              application/json:
                type: PropertyList
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/drive.readonly'
        (oas-tags):
          - properties
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      post:
        displayName: drive.properties.insert
        description: 'Adds a property to a file, or updates it if it already exists.'
        body:
          application/json:
            type: Property
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: Property
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/drive.metadata'
        (oas-tags):
          - properties
      '/{propertyKey}':
        displayName: '{propertyKey}'
        uriParameters:
          propertyKey:
            description: The key of the property.
            required: true
            type: string
            displayName: The key of the property.
        delete:
          displayName: drive.properties.delete
          description: Deletes a property.
          responses:
            '200':
              description: Successful response
          queryParameters:
            visibility:
              default: private
              description: The visibility of the property.
              type: string
              displayName: The visibility of the property.
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.metadata'
          (oas-tags):
            - properties
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        get:
          displayName: drive.properties.get
          description: Gets a property by its key.
          responses:
            '200':
              body:
                application/json:
                  type: Property
              description: Successful response
          queryParameters:
            visibility:
              default: private
              description: The visibility of the property.
              type: string
              displayName: The visibility of the property.
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.readonly'
          (oas-tags):
            - properties
        patch:
          displayName: drive.properties.patch
          description: 'Updates a property, or adds it if it doesn''t exist. This method supports patch semantics.'
          body:
            application/json:
              type: Property
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Property
              description: Successful response
          queryParameters:
            visibility:
              default: private
              description: The visibility of the property.
              type: string
              displayName: The visibility of the property.
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.metadata'
          (oas-tags):
            - properties
        put:
          displayName: drive.properties.update
          description: 'Updates a property, or adds it if it doesn''t exist.'
          body:
            application/json:
              type: Property
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Property
              description: Successful response
          queryParameters:
            visibility:
              default: private
              description: The visibility of the property.
              type: string
              displayName: The visibility of the property.
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.metadata'
          (oas-tags):
            - properties
    /comments:
      displayName: comments
      post:
        displayName: drive.comments.insert
        description: Creates a new comment on the given file.
        body:
          application/json:
            type: Comment
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: Comment
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/drive.file'
        (oas-tags):
          - comments
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      '/{commentId}':
        displayName: '{commentId}'
        uriParameters:
          commentId:
            description: The ID of the comment.
            required: true
            type: string
            displayName: The ID of the comment.
        get:
          displayName: drive.comments.get
          description: Gets a comment by ID.
          responses:
            '200':
              body:
                application/json:
                  type: Comment
              description: Successful response
          queryParameters:
            includeDeleted:
              default: false
              description: 'If set, this will succeed when retrieving a deleted comment, and will include any deleted replies.'
              type: boolean
              displayName: 'If set, this will succeed when retrieving a deleted comment, and will include any deleted replies.'
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.readonly'
          (oas-tags):
            - comments
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        put:
          displayName: drive.comments.update
          description: Updates an existing comment.
          body:
            application/json:
              type: Comment
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Comment
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.file'
          (oas-tags):
            - comments
        /replies:
          displayName: replies
          post:
            displayName: drive.replies.insert
            description: Creates a new reply to the given comment.
            body:
              application/json:
                type: CommentReply
                (oas-body-name): body
            responses:
              '200':
                body:
                  application/json:
                    type: CommentReply
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/drive.file'
            (oas-tags):
              - replies
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
          '/{replyId}':
            displayName: '{replyId}'
            uriParameters:
              replyId:
                description: The ID of the reply.
                required: true
                type: string
                displayName: The ID of the reply.
            get:
              displayName: drive.replies.get
              description: Gets a reply.
              responses:
                '200':
                  body:
                    application/json:
                      type: CommentReply
                  description: Successful response
              queryParameters:
                includeDeleted:
                  default: false
                  description: 'If set, this will succeed when retrieving a deleted reply.'
                  type: boolean
                  displayName: 'If set, this will succeed when retrieving a deleted reply.'
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/drive.readonly'
              (oas-tags):
                - replies
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
            put:
              displayName: drive.replies.update
              description: Updates an existing reply.
              body:
                application/json:
                  type: CommentReply
                  (oas-body-name): body
              responses:
                '200':
                  body:
                    application/json:
                      type: CommentReply
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/drive.file'
              (oas-tags):
                - replies
            patch:
              displayName: drive.replies.patch
              description: Updates an existing reply. This method supports patch semantics.
              body:
                application/json:
                  type: CommentReply
                  (oas-body-name): body
              responses:
                '200':
                  body:
                    application/json:
                      type: CommentReply
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/drive.file'
              (oas-tags):
                - replies
            delete:
              displayName: drive.replies.delete
              description: Deletes a reply.
              responses:
                '200':
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/drive.file'
              (oas-tags):
                - replies
          get:
            displayName: drive.replies.list
            description: Lists all of the replies to a comment.
            responses:
              '200':
                body:
                  application/json:
                    type: CommentReplyList
                description: Successful response
            queryParameters:
              includeDeleted:
                default: false
                description: 'If set, all replies, including deleted replies (with content stripped) will be returned.'
                type: boolean
                displayName: 'If set, all replies, including deleted replies (with content stripped) will be returned.'
              maxResults:
                default: 20
                description: 'The maximum number of replies to include in the response, used for paging.'
                maximum: 100
                minimum: 0
                type: integer
                displayName: 'The maximum number of replies to include in the response, used for paging.'
              pageToken:
                description: 'The continuation token, used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
                type: string
                displayName: 'The continuation token, used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/drive.readonly'
            (oas-tags):
              - replies
        delete:
          displayName: drive.comments.delete
          description: Deletes a comment.
          responses:
            '200':
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.file'
          (oas-tags):
            - comments
        patch:
          displayName: drive.comments.patch
          description: Updates an existing comment. This method supports patch semantics.
          body:
            application/json:
              type: Comment
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Comment
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/drive.file'
          (oas-tags):
            - comments
      get:
        displayName: drive.comments.list
        description: Lists a file's comments.
        responses:
          '200':
            body:
              application/json:
                type: CommentList
            description: Successful response
        queryParameters:
          includeDeleted:
            default: false
            description: 'If set, all comments and replies, including deleted comments and replies (with content stripped) will be returned.'
            type: boolean
            displayName: 'If set, all comments and replies, including deleted comments and replies (with content stripped) will be returned.'
          maxResults:
            default: 20
            description: 'The maximum number of discussions to include in the response, used for paging.'
