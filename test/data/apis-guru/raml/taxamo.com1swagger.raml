#%RAML 1.0
title: Taxamo
version: '1'
baseUri: 'https://api.taxamo.com/'
mediaType: application/json
protocols:
  - HTTPS
description: 'Taxamoâ€™s elegant suite of APIs and comprehensive reporting dashboard enables digital merchants to easily comply with EU regulatory requirements on tax calculation, evidence collection, tax return creation and data storage.'
(oas-tags-definition):
  - description: Dictionaries
    name: dictionaries
  - description: GeoIP
    name: geoip
  - description: Invoices
    name: invoices
  - description: Payments
    name: payments
  - description: Refunds
    name: refunds
  - description: Reporting
    name: reporting
  - description: Settlement
    name: settlement
  - description: Statistics
    name: statistics
  - description: Tax
    name: tax
  - description: Transactions
    name: transactions
  - description: Verification
    name: verification
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/http_i.imgur.com_C7ouKJt.png'
  (oas-x-origin):
    format: swagger
    url: 'https://api.taxamo.com/swagger'
    version: '1.2'
  (oas-x-preferred): true
  (oas-x-providerName): taxamo.com
securitySchemes:
  apiKey:
    type: Pass Through
    describedBy:
      headers:
        Token:
          type: string
/api:
  displayName: api
  /v1:
    displayName: v1
    /tax:
      displayName: tax
      /location:
        displayName: location
        /calculate:
          displayName: calculate
          get:
            displayName: calculateTaxLocation
            (oas-summary): Calculate location
            responses:
              '200':
                body:
                  application/json:
                    type: calculateTaxLocationOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            queryParameters:
              billing_country_code:
                description: Billing two letter ISO country code.
                required: false
                type: string
                displayName: Billing two letter ISO country code.
              buyer_credit_card_prefix:
                description: First 6 digits of buyer's credit card prefix.
                required: false
                type: string
                displayName: First 6 digits of buyer's credit card prefix.
            (oas-tags):
              - tax
      /vat_numbers:
        displayName: vat_numbers
        '/{tax_number}':
          displayName: '{tax_number}'
          /validate:
            displayName: validate
            get:
              displayName: validateTaxNumber
              (oas-summary): Validate VAT number
              responses:
                '200':
                  body:
                    application/json:
                      type: validateTaxNumberOut
                  description: OK
                '400':
                  description: 'Validation failed, see JSON body response for details.'
                '401':
                  description: Incorrect token
              queryParameters:
                country_code:
                  description: Two-letter ISO country code.
                  required: false
                  type: string
                  displayName: Two-letter ISO country code.
              (oas-tags):
                - tax
      /calculate:
        displayName: calculate
        post:
          displayName: calculateTax
          (oas-summary): Calculate tax
          body:
            application/json:
              type: calculateTaxIn
              (oas-body-name): input
              description: Input
          responses:
            '200':
              body:
                application/json:
                  type: calculateTaxOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - tax
        get:
          displayName: calculateSimpleTax
          (oas-summary): Simple tax
          responses:
            '200':
              body:
                application/json:
                  type: calculateSimpleTaxOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          queryParameters:
            product_type:
              description: 'Product type, according to dictionary /dictionaries/product_types. '
              required: false
              type: string
              displayName: 'Product type, according to dictionary /dictionaries/product_types. '
            invoice_address_city:
              description: Invoice address/postal_code
              required: false
              type: string
              displayName: Invoice address/postal_code
            buyer_credit_card_prefix:
              description: First 6 digits of buyer's credit card prefix.
              required: false
              type: string
              displayName: First 6 digits of buyer's credit card prefix.
            currency_code:
              description: Currency code for transaction - e.g. EUR.
              required: true
              type: string
              displayName: Currency code for transaction - e.g. EUR.
            invoice_address_region:
              description: Invoice address/region
              required: false
              type: string
              displayName: Invoice address/region
            unit_price:
              description: Unit price.
              required: false
              type: number
              displayName: Unit price.
            quantity:
              description: Quantity Defaults to 1.
              required: false
              type: number
              displayName: Quantity Defaults to 1.
            buyer_tax_number:
              description: ' Buyer''s tax number - EU VAT number for example. If using EU VAT number, it is possible to provide country code in it (e.g. IE1234567X) or simply use billing_country_code field for that. In the first case, if billing_country_code value was provided, it will be overwritten with country code value extracted from VAT number - but only if the VAT has been verified properly.'
              required: false
              type: string
              displayName: ' Buyer''s tax number - EU VAT number for example. If using EU VAT number, it is possible to provide country code in it (e.g. IE1234567X) or simply use billing_country_code field for that. In the first case, if billing_country_code value was provided, it will be overwritten with country code value extracted from VAT number - but only if the VAT has been verified properly.'
            force_country_code:
              description: 'Two-letter ISO country code, e.g. FR. Use it to force country code for tax calculation.'
              required: false
              type: string
              displayName: 'Two-letter ISO country code, e.g. FR. Use it to force country code for tax calculation.'
            order_date:
              description: 'Order date in yyyy-MM-dd format, in merchant''s timezone. If provided by the API caller, no timezone conversion is performed. Default value is current date and time. When using public token, the default value is used.'
              required: false
              type: string
              displayName: 'Order date in yyyy-MM-dd format, in merchant''s timezone. If provided by the API caller, no timezone conversion is performed. Default value is current date and time. When using public token, the default value is used.'
            amount:
              description: Amount. Required if total amount or both unit price and quantity are not provided.
              required: false
              type: number
              displayName: Amount. Required if total amount or both unit price and quantity are not provided.
            billing_country_code:
              description: Billing two letter ISO country code.
              required: false
              type: string
              displayName: Billing two letter ISO country code.
            invoice_address_postal_code:
              description: Invoice address/postal_code
              required: false
              type: string
              displayName: Invoice address/postal_code
            total_amount:
              description: Total amount. Required if amount or both unit price and quantity are not provided.
              required: false
              type: number
              displayName: Total amount. Required if amount or both unit price and quantity are not provided.
            tax_deducted:
              description: 'If the transaction is in a country supported by Taxamo, but the tax is not calculated due to merchant settings or EU B2B transaction for example.'
              required: false
              type: boolean
              displayName: 'If the transaction is in a country supported by Taxamo, but the tax is not calculated due to merchant settings or EU B2B transaction for example.'
          (oas-tags):
            - tax
    /verification:
      displayName: verification
      /sms:
        displayName: sms
        '/{token}':
          displayName: '{token}'
          uriParameters:
            token:
              description: Provided token.
              required: true
              type: string
              displayName: Provided token.
          get:
            displayName: verifySMSToken
            (oas-summary): Verify SMS token
            responses:
              '200':
                body:
                  application/json:
                    type: verifySMSTokenOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            (oas-tags):
              - verification
        post:
          displayName: createSMSToken
          (oas-summary): Create SMS token
          body:
            application/json:
              type: createSMSTokenIn
              (oas-body-name): input
              description: Input
          responses:
            '200':
              body:
                application/json:
                  type: createSMSTokenOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - verification
    /dictionaries:
      displayName: dictionaries
      /countries:
        displayName: countries
        get:
          displayName: getCountriesDict
          (oas-summary): Countries
          responses:
            '200':
              body:
                application/json:
                  type: getCountriesDictOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          queryParameters:
            tax_supported:
              description: Should only countries with tax supported be listed?
              required: false
              type: boolean
              displayName: Should only countries with tax supported be listed?
          (oas-tags):
            - dictionaries
      /product_types:
        displayName: product_types
        get:
          displayName: getProductTypesDict
          (oas-summary): Product types
          responses:
            '200':
              body:
                application/json:
                  type: getProductTypesDictOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - dictionaries
      /currencies:
        displayName: currencies
        get:
          displayName: getCurrenciesDict
          (oas-summary): Currencies
          responses:
            '200':
              body:
                application/json:
                  type: getCurrenciesDictOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - dictionaries
    /transactions:
      displayName: transactions
      get:
        displayName: listTransactions
        (oas-summary): Browse transactions
        responses:
          '200':
            body:
              application/json:
                type: listTransactionsOut
            description: OK
          '400':
            description: 'Validation failed, see JSON body response for details.'
          '401':
            description: Incorrect token
        queryParameters:
          filter_text:
            description: Filtering expression
            required: false
            type: string
            displayName: Filtering expression
          offset:
            description: Offset
            required: false
            type: integer
            displayName: Offset
          has_note:
            description: Return only transactions with a note field set.
            required: false
            type: boolean
            displayName: Return only transactions with a note field set.
          key_or_custom_id:
            description: Taxamo provided transaction key or custom id
            required: false
            type: string
            displayName: Taxamo provided transaction key or custom id
          currency_code:
            description: Three letter ISO currency code.
            required: false
            type: string
            displayName: Three letter ISO currency code.
          order_date_to:
            description: Order date to in yyyy-MM-dd format.
            required: false
            type: string
            displayName: Order date to in yyyy-MM-dd format.
          sort_reverse:
            description: 'If true, results are sorted in descending order.'
            required: false
            type: boolean
            displayName: 'If true, results are sorted in descending order.'
          limit:
            description: 'Limit (no more than 1000, defaults to 100).'
            required: false
            type: integer
            displayName: 'Limit (no more than 1000, defaults to 100).'
          invoice_number:
            description: Transaction invoice number.
            required: false
            type: string
            displayName: Transaction invoice number.
          tax_country_codes:
            description: Comma separated list of two letter ISO tax country codes.
            required: false
            type: string
            displayName: Comma separated list of two letter ISO tax country codes.
          statuses:
            description: 'Comma separated list of of transaction statuses. ''N'' - unconfirmed transaction, ''C'' - confirmed transaction.'
            required: false
            type: string
            displayName: 'Comma separated list of of transaction statuses. ''N'' - unconfirmed transaction, ''C'' - confirmed transaction.'
          original_transaction_key:
            description: Taxamo provided original transaction key
            required: false
            type: string
            displayName: Taxamo provided original transaction key
          order_date_from:
            description: Order date from in yyyy-MM-dd format.
            required: false
            type: string
            displayName: Order date from in yyyy-MM-dd format.
          total_amount_greater_than:
            description: Return only transactions with total amount greater than given number. Transactions with total amount equal to a given number (e.g. 0) are not returned.
            required: false
            type: string
            displayName: Return only transactions with total amount greater than given number. Transactions with total amount equal to a given number (e.g. 0) are not returned.
          format:
            description: Output format - supports 'csv' value for this operation.
            required: false
            type: string
            displayName: Output format - supports 'csv' value for this operation.
          total_amount_less_than:
            description: Return only transactions with total amount less than a given number. Transactions with total amount equal to a given number (e.g. 1) are not returned.
            required: false
            type: string
            displayName: Return only transactions with total amount less than a given number. Transactions with total amount equal to a given number (e.g. 1) are not returned.
          tax_country_code:
            description: Two letter ISO tax country code.
            required: false
            type: string
            displayName: Two letter ISO tax country code.
        (oas-tags):
          - transactions
      post:
        displayName: createTransaction
        (oas-summary): Store transaction
        body:
          application/json:
            type: createTransactionIn
            (oas-body-name): input
            description: Input
        responses:
          '200':
            body:
              application/json:
                type: createTransactionOut
            description: OK
          '400':
            description: 'Validation failed, see JSON body response for details.'
          '401':
            description: Incorrect token
        (oas-tags):
          - transactions
      '/{key}':
        displayName: '{key}'
        uriParameters:
          key:
            description: Transaction key.
            required: true
            type: string
            displayName: Transaction key.
        delete:
          displayName: cancelTransaction
          (oas-summary): Delete transaction
          responses:
            '200':
              body:
                application/json:
                  type: cancelTransactionOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - transactions
        get:
          displayName: getTransaction
          (oas-summary): Retrieve transaction data.
          responses:
            '200':
              body:
                application/json:
                  type: getTransactionOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - transactions
        put:
          displayName: updateTransaction
          (oas-summary): Update transaction
          body:
            application/json:
              type: updateTransactionIn
              (oas-body-name): input
              description: Input
          responses:
            '200':
              body:
                application/json:
                  type: updateTransactionOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - transactions
        /confirm:
          displayName: confirm
          post:
            displayName: confirmTransaction
            (oas-summary): Confirm transaction
            body:
              application/json:
                type: confirmTransactionIn
                (oas-body-name): input
                description: Input
            responses:
              '200':
                body:
                  application/json:
                    type: confirmTransactionOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            (oas-tags):
              - transactions
        /invoice:
          displayName: invoice
          /refunds:
            displayName: refunds
            '/{refund_note_number}':
              displayName: '{refund_note_number}'
              /send_email:
                displayName: send_email
                post:
                  displayName: emailRefund
                  (oas-summary): Email credit note
                  body:
                    application/json:
                      type: emailRefundIn
                      (oas-body-name): input
                      description: Input
                  responses:
                    '200':
                      body:
                        application/json:
                          type: emailRefundOut
                      description: OK
                    '400':
                      description: 'Validation failed, see JSON body response for details.'
                    '401':
                      description: Incorrect token
                  (oas-tags):
                    - invoices
          /send_email:
            displayName: send_email
            post:
              displayName: emailInvoice
              (oas-summary): Email invoice
              body:
                application/json:
                  type: emailInvoiceIn
                  (oas-body-name): input
                  description: Input
              responses:
                '200':
                  body:
                    application/json:
                      type: emailInvoiceOut
                  description: OK
                '400':
                  description: 'Validation failed, see JSON body response for details.'
                '401':
                  description: Incorrect token
              (oas-tags):
                - invoices
        /payments:
          displayName: payments
          get:
            displayName: listPayments
            (oas-summary): List payments
            responses:
              '200':
                body:
                  application/json:
                    type: listPaymentsOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            queryParameters:
              limit:
                description: 'Max record count (no more than 100, defaults to 10).'
                required: false
                type: string
                displayName: 'Max record count (no more than 100, defaults to 10).'
              offset:
                description: 'How many records need to be skipped, defaults to 0.'
                required: false
                type: string
                displayName: 'How many records need to be skipped, defaults to 0.'
            (oas-tags):
              - payments
          post:
            displayName: createPayment
            (oas-summary): Register a payment
            body:
              application/json:
                type: createPaymentIn
                (oas-body-name): input
                description: Input
            responses:
              '200':
                body:
                  application/json:
                    type: createPaymentOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            (oas-tags):
              - payments
          /capture:
            displayName: capture
            post:
              displayName: capturePayment
              (oas-summary): Capture payment
              responses:
                '200':
                  body:
                    application/json:
                      type: capturePaymentOut
                  description: OK
                '400':
                  description: 'Validation failed, see JSON body response for details.'
                '401':
                  description: Incorrect token
              (oas-tags):
                - payments
        /unconfirm:
          displayName: unconfirm
          post:
            displayName: unconfirmTransaction
            (oas-summary): Un-confirm the transaction
            body:
              application/json:
                type: unconfirmTransactionIn
                (oas-body-name): input
                description: Input
            responses:
              '200':
                body:
                  application/json:
                    type: unconfirmTransactionOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            (oas-tags):
              - transactions
        /refunds:
          displayName: refunds
          post:
            displayName: createRefund
            (oas-summary): Create a refund
            body:
              application/json:
                type: createRefundIn
                (oas-body-name): input
                description: Input
            responses:
              '200':
                body:
                  application/json:
                    type: createRefundOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            (oas-tags):
              - refunds
          get:
            displayName: listRefunds
            (oas-summary): Get transaction refunds
            responses:
              '200':
                body:
                  application/json:
                    type: listRefundsOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            (oas-tags):
              - refunds
    /geoip:
      displayName: geoip
      '/{ip}':
        displayName: '{ip}'
        uriParameters:
          ip:
            description: IP address.
            required: true
            type: string
            displayName: IP address.
        get:
          displayName: locateGivenIP
          (oas-summary): Locate provided IP
          responses:
            '200':
              body:
                application/json:
                  type: locateGivenIPOut
              description: OK
            '400':
              description: 'Validation failed, see JSON body response for details.'
            '401':
              description: Incorrect token
          (oas-tags):
            - geoip
      get:
        displayName: locateMyIP
        (oas-summary): Locate IP
        responses:
          '200':
            body:
              application/json:
                type: locateMyIPOut
            description: OK
          '400':
            description: 'Validation failed, see JSON body response for details.'
          '401':
            description: Incorrect token
        (oas-tags):
          - geoip
    /reports:
      displayName: reports
      /eu:
        displayName: eu
        /vies:
          displayName: vies
          get:
            displayName: getEuViesReport
            (oas-summary): Calculate EU VIES report
            responses:
              '200':
                body:
                  application/json:
                    type: getEuViesReportOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            queryParameters:
              period_length:
                description: 'Length of report period. ''month'', ''quarter'' and ''year'' values are accepted. Required only if Large Filer Format is requested.'
                required: false
                type: string
                displayName: 'Length of report period. ''month'', ''quarter'' and ''year'' values are accepted. Required only if Large Filer Format is requested.'
              lff_sequence_number:
                description: Sequence number used to generate report in Large Filer Format. If not specified then '0000000001' will be used.
                required: false
                type: string
                displayName: Sequence number used to generate report in Large Filer Format. If not specified then '0000000001' will be used.
              transformation:
                description: Which transformation should be applied. Please note that transformation will be applied only for xml and csv formats.
                required: false
                type: string
                displayName: Which transformation should be applied. Please note that transformation will be applied only for xml and csv formats.
              currency_code:
                description: 'ISO 3-letter currency code, e.g. EUR or USD. Defaults to the one assigned to MOSS calculations for a given country code.'
                required: false
                type: string
                displayName: 'ISO 3-letter currency code, e.g. EUR or USD. Defaults to the one assigned to MOSS calculations for a given country code.'
              end_month:
                description: Period end month in yyyy-MM format.
                required: true
                type: string
                displayName: Period end month in yyyy-MM format.
              tax_id:
                description: 'MOSS-assigned tax ID - if not provided, merchant''s national tax number will be used.'
                required: false
                type: string
                displayName: 'MOSS-assigned tax ID - if not provided, merchant''s national tax number will be used.'
              start_month:
                description: Period start month in yyyy-MM format.
                required: true
                type: string
                displayName: Period start month in yyyy-MM format.
              eu_country_code:
                description: ISO 2-letter country code which will be used for determining which country is domestic.
                required: true
                type: string
                displayName: ISO 2-letter country code which will be used for determining which country is domestic.
              fx_date_type:
                description: Which date should be used for FX.
                required: false
                type: string
                displayName: Which date should be used for FX.
              format:
                description: 'Output format. ''xml'', ''csv'' and ''lff'' (only for Ireland) values are accepted as well'
                required: false
                type: string
                displayName: 'Output format. ''xml'', ''csv'' and ''lff'' (only for Ireland) values are accepted as well'
            (oas-tags):
              - reporting
      /domestic:
        displayName: domestic
        /summary:
          displayName: summary
          get:
            displayName: getDomesticSummaryReport
            (oas-summary): Calculate domestic summary
            responses:
              '200':
                body:
                  application/json:
                    type: getDomesticSummaryReportOut
                description: OK
              '400':
                description: 'Validation failed, see JSON body response for details.'
              '401':
                description: Incorrect token
            queryParameters:
              format:
                description: Output format. 'xml' and 'csv' values are accepted. Default format - json
                required: false
                type: string
                displayName: Output format. 'xml' and 'csv' values are accepted. Default format - json
              country_code:
                description: ISO 2-letter country code which will be used for determining which country is domestic.
                required: true
                type: string
                displayName: ISO 2-letter country code which will be used for determining which country is domestic.
              currency_code:
                description: 'ISO 3-letter currency code, e.g. EUR or USD. Defaults to the one assigned to MOSS calculations for a given country code.'
