                          body:
                            application/json:
                              type: ErrorResponse
                          description: CDN error response describing why the operation failed.
                        responses:
                          '200':
                            body:
                              application/json:
                                type: OriginListResult
                            description: OK. The request has succeeded.
                        (oas-tags):
                          - Origins
                      '/{originName}':
                        displayName: '{originName}'
                        uriParameters:
                          originName:
                            description: Name of the origin which is unique within the endpoint.
                            required: true
                            type: string
                            displayName: Name of the origin which is unique within the endpoint.
                        get:
                          displayName: Origins_Get
                          description: Gets an existing CDN origin within an endpoint.
                          is:
                            - apiVersionParameter
                          (oas-responses-default):
                            body:
                              application/json:
                                type: ErrorResponse
                            description: CDN error response describing why the operation failed.
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: Origin
                              description: OK. The request has succeeded.
                          (oas-tags):
                            - Origins
                        patch:
                          (oas-x-ms-long-running-operation): true
                          displayName: Origins_Update
                          description: Updates an existing CDN origin within an endpoint.
                          is:
                            - apiVersionParameter
                          body:
                            application/json:
                              type: OriginUpdateParameters
                              (oas-body-name): originUpdateProperties
                              description: Origin properties
                          (oas-responses-default):
                            body:
                              application/json:
                                type: ErrorResponse
                            description: CDN error response describing why the operation failed.
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: Origin
                              description: OK. The request has succeeded.
                            '202':
                              body:
                                application/json:
                                  type: Origin
                              description: Accepted and the operation will complete asynchronously.
                          (oas-tags):
                            - Origins
                    uriParameters:
                      endpointName:
                        description: Name of the endpoint under the profile which is unique globally.
                        required: true
                        type: string
                        displayName: Name of the endpoint under the profile which is unique globally.
                    get:
                      displayName: Endpoints_Get
                      description: 'Gets an existing CDN endpoint with the specified endpoint name under the specified subscription, resource group and profile.'
                      is:
                        - apiVersionParameter
                      (oas-responses-default):
                        body:
                          application/json:
                            type: ErrorResponse
                        description: CDN error response describing why the operation failed.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: Endpoint
                          description: OK. The request has succeeded.
                      (oas-tags):
                        - Endpoints
                    put:
                      (oas-x-ms-long-running-operation): true
                      displayName: Endpoints_Create
                      description: Creates a new CDN endpoint with the specified parameters.
                      is:
                        - apiVersionParameter
                      body:
                        application/json:
                          type: Endpoint
                          (oas-body-name): endpoint
                          description: Endpoint properties
                      (oas-responses-default):
                        body:
                          application/json:
                            type: ErrorResponse
                        description: CDN error response describing why the operation failed.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: Endpoint
                          description: OK. The request has succeeded.
                        '201':
                          body:
                            application/json:
                              type: Endpoint
                          description: Created. The request has been fulfilled and a new endpoint has been created.
                        '202':
                          body:
                            application/json:
                              type: Endpoint
                          description: Accepted and the operation will complete asynchronously.
                      (oas-tags):
                        - Endpoints
                    /validateCustomDomain:
                      displayName: validateCustomDomain
                      post:
                        displayName: Endpoints_ValidateCustomDomain
                        description: Validates a custom domain mapping to ensure it maps to the correct CNAME in DNS.
                        is:
                          - apiVersionParameter
                        body:
                          application/json:
                            type: ValidateCustomDomainInput
                            (oas-body-name): customDomainProperties
                            description: Custom domain to validate.
                        (oas-responses-default):
                          body:
                            application/json:
                              type: ErrorResponse
                          description: CDN error response describing why the operation failed.
                        responses:
                          '200':
                            body:
                              application/json:
                                type: ValidateCustomDomainOutput
                            description: OK. The request has succeeded.
                        (oas-tags):
                          - Endpoints
                    /stop:
                      displayName: stop
                      post:
                        (oas-x-ms-long-running-operation): true
                        displayName: Endpoints_Stop
                        description: Stops an existing running CDN endpoint.
                        is:
                          - apiVersionParameter
                        (oas-responses-default):
                          body:
                            application/json:
                              type: ErrorResponse
                          description: CDN error response describing why the operation failed.
                        responses:
                          '202':
                            body:
                              application/json:
                                type: Endpoint
                            description: Accepted and the operation will complete asynchronously.
                        (oas-tags):
                          - Endpoints
                    /start:
                      displayName: start
                      post:
                        (oas-x-ms-long-running-operation): true
                        displayName: Endpoints_Start
                        description: Starts an existing stopped CDN endpoint.
                        is:
                          - apiVersionParameter
                        (oas-responses-default):
                          body:
                            application/json:
                              type: ErrorResponse
                          description: CDN error response describing why the operation failed.
                        responses:
                          '202':
                            body:
                              application/json:
                                type: Endpoint
                            description: Accepted and the operation will complete asynchronously.
                        (oas-tags):
                          - Endpoints
                    /purge:
                      displayName: purge
                      post:
                        (oas-x-ms-long-running-operation): true
                        displayName: Endpoints_PurgeContent
                        description: Forcibly purges CDN endpoint content.
                        is:
                          - apiVersionParameter
                        body:
                          application/json:
                            type: PurgeParameters
                            (oas-body-name): contentFilePaths
                            description: The path to the content to be purged. Path can describe a file or directory using the wildcard. e.g. '/my/directory/*' or '/my/file.exe/'
                        (oas-responses-default):
                          body:
                            application/json:
                              type: ErrorResponse
                          description: CDN error response describing why the operation failed.
                        responses:
                          '202':
                            description: Accepted and the operation will complete asynchronously.
                        (oas-tags):
                          - Endpoints
                    patch:
                      (oas-x-ms-long-running-operation): true
                      displayName: Endpoints_Update
                      description: 'Updates an existing CDN endpoint with the specified parameters. Only tags and OriginHostHeader can be updated after creating an endpoint. To update origins, use the Update Origin operation. To update custom domains, use the Update Custom Domain operation.'
                      is:
                        - apiVersionParameter
                      body:
                        application/json:
                          type: EndpointUpdateParameters
                          (oas-body-name): endpointUpdateProperties
                          description: Endpoint update properties
                      (oas-responses-default):
                        body:
                          application/json:
                            type: ErrorResponse
                        description: CDN error response describing why the operation failed
                      responses:
                        '200':
                          body:
                            application/json:
                              type: Endpoint
                          description: OK. The request has succeeded.
                        '202':
                          body:
                            application/json:
                              type: Endpoint
                          description: Accepted and  the operation will complete asynchronously
                      (oas-tags):
                        - Endpoints
                    delete:
                      (oas-x-ms-long-running-operation): true
                      displayName: Endpoints_Delete
                      description: Deletes an existing CDN endpoint with the specified parameters.
                      is:
                        - apiVersionParameter
                      (oas-responses-default):
                        body:
                          application/json:
                            type: ErrorResponse
                        description: CDN error response describing why the operation failed.
                      responses:
                        '202':
                          description: Accepted.
                        '204':
                          description: No Content.
                      (oas-tags):
                        - Endpoints
                  get:
                    (oas-x-ms-pageable):
                      nextLinkName: nextLink
                    displayName: Endpoints_ListByProfile
                    description: Lists existing CDN endpoints.
                    is:
                      - apiVersionParameter
                    (oas-responses-default):
                      body:
                        application/json:
                          type: ErrorResponse
                      description: CDN error response describing why the operation failed.
                    responses:
                      '200':
                        body:
                          application/json:
                            type: EndpointListResult
                        description: OK. The request has succeeded.
                    (oas-tags):
                      - Endpoints
                /generateSsoUri:
                  displayName: generateSsoUri
                  post:
                    displayName: Profiles_GenerateSsoUri
                    description: 'Generates a dynamic SSO URI used to sign in to the CDN supplemental portal. Supplemnetal portal is used to configure advanced feature capabilities that are not yet available in the Azure portal, such as core reports in a standard profile; rules engine, advanced HTTP reports, and real-time stats and alerts in a premium profile. The SSO URI changes approximately every 10 minutes.'
                    is:
                      - apiVersionParameter
                    (oas-responses-default):
                      body:
                        application/json:
                          type: ErrorResponse
                      description: CDN error response describing why the operation failed.
                    responses:
                      '200':
                        body:
                          application/json:
                            type: SsoUri
                        description: OK. The request has succeeded.
                    (oas-tags):
                      - Profiles
                uriParameters:
                  profileName:
                    description: Name of the CDN profile which is unique within the resource group.
                    required: true
                    type: string
                    displayName: Name of the CDN profile which is unique within the resource group.
                delete:
                  (oas-x-ms-long-running-operation): true
                  displayName: Profiles_Delete
                  description: 'Deletes an existing CDN profile with the specified parameters. Deleting a profile will result in the deletion of all subresources including endpoints, origins and custom domains.'
                  is:
                    - apiVersionParameter
                  (oas-responses-default):
                    body:
                      application/json:
                        type: ErrorResponse
                    description: CDN error response describing why the operation failed.
                  responses:
                    '202':
                      description: Accepted. The request has been accepted for processing and the operation will complete asynchronously.
                    '204':
                      description: No Content. The request has been accepted but the profile was not found.
                  (oas-tags):
                    - Profiles
                patch:
                  (oas-x-ms-long-running-operation): true
                  displayName: Profiles_Update
                  description: Updates an existing CDN profile with the specified profile name under the specified subscription and resource group.
                  is:
                    - apiVersionParameter
                  body:
                    application/json:
                      type: ProfileUpdateParameters
                      (oas-body-name): profileUpdateParameters
                      description: Profile properties needed to update an existing profile.
                  (oas-responses-default):
                    body:
                      application/json:
                        type: ErrorResponse
                    description: CDN error response describing why the operation failed.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Profile
                      description: OK. The request has succeeded.
                    '202':
                      body:
                        application/json:
                          type: Profile
                      description: Accepted. The request has been accepted for processing and the operation will complete asynchronously.
                  (oas-tags):
                    - Profiles
                get:
                  displayName: Profiles_Get
                  description: Gets a CDN profile with the specified profile name under the specified subscription and resource group.
                  is:
                    - apiVersionParameter
                  (oas-responses-default):
                    body:
                      application/json:
                        type: ErrorResponse
                    description: CDN error response describing why the operation failed.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Profile
                      description: OK. The request has succeeded.
                  (oas-tags):
                    - Profiles
                put:
                  (oas-x-ms-long-running-operation): true
                  displayName: Profiles_Create
                  description: Creates a new CDN profile with a profile name under the specified subscription and resource group.
                  is:
                    - apiVersionParameter
                  body:
                    application/json:
                      type: Profile
                      (oas-body-name): profile
                      description: Profile properties needed to create a new profile.
                  (oas-responses-default):
                    body:
                      application/json:
                        type: ErrorResponse
                    description: CDN error response describing why the operation failed.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Profile
                      description: OK. The request has succeeded.
                    '201':
                      body:
                        application/json:
                          type: Profile
                      description: Created. The request has been fulfilled and a new profile has been created.
                    '202':
                      body:
                        application/json:
                          type: Profile
                      description: Accepted. The request has been accepted for processing and the operation will complete asynchronously.
                  (oas-tags):
                    - Profiles
              get:
                (oas-x-ms-pageable):
                  nextLinkName: nextLink
                displayName: Profiles_ListByResourceGroup
                description: Lists all the CDN profiles within a resource group.
                is:
                  - apiVersionParameter
                (oas-responses-default):
                  body:
                    application/json:
                      type: ErrorResponse
                  description: CDN error response describing why the operation failed.
                responses:
                  '200':
                    body:
                      application/json:
                        type: ProfileListResult
                    description: OK. The request has succeeded.
                (oas-tags):
                  - Profiles
    /providers:
      displayName: providers
      /Microsoft.Cdn:
        displayName: Microsoft.Cdn
        /profiles:
          displayName: profiles
          get:
            (oas-x-ms-pageable):
              nextLinkName: nextLink
            displayName: Profiles_List
            description: Lists all the CDN profiles within an Azure subscription.
            is:
              - apiVersionParameter
            (oas-responses-default):
              body:
                application/json:
                  type: ErrorResponse
              description: CDN error response describing why the operation failed.
            responses:
              '200':
                body:
                  application/json:
                    type: ProfileListResult
                description: OK. The request has succeeded.
            (oas-tags):
              - Profiles
/providers:
  displayName: providers
  /Microsoft.Cdn:
    displayName: Microsoft.Cdn
    /checkNameAvailability:
      displayName: checkNameAvailability
      post:
        displayName: CheckNameAvailability
        description: 'Check the availability of a resource name without creating the resource. This is needed for resources where name is globally unique, such as a CDN endpoint.'
        is:
          - apiVersionParameter
        body:
          application/json:
            type: CheckNameAvailabilityInput
            (oas-body-name): checkNameAvailabilityInput
            description: Input to check.
        (oas-responses-default):
          body:
            application/json:
              type: ErrorResponse
          description: CDN error response describing why the operation failed.
        responses:
          '200':
            body:
              application/json:
                type: CheckNameAvailabilityOutput
            description: OK. The request has succeeded.
        (oas-tags):
          - CheckNameAvailability
    /operations:
      displayName: operations
      get:
        (oas-x-ms-pageable):
          nextLinkName: nextLink
        displayName: ListOperations
        description: Lists all of the available CDN REST API operations.
        is:
          - apiVersionParameter
        (oas-responses-default):
          body:
            application/json:
              type: ErrorResponse
          description: CDN error response describing why the operation failed.
        responses:
          '200':
            body:
              application/json:
                type: OperationListResult
            description: OK. The request has succeeded.
        (oas-tags):
          - Operations
types:
  CheckNameAvailabilityInput:
    description: Input of CheckNameAvailability API.
    properties:
      name:
        description: The resource name to validate.
        type: string
      type:
        description: The type of the resource whose name is to be validated.
        type: ResourceType
  CheckNameAvailabilityOutput:
    description: Output of check name availability API.
    properties:
      message:
        description: The detailed error message describing why the name is not available.
        type: string
        required: false
      nameAvailable:
        description: Indicates whether the name is available.
        type: boolean
        required: false
      reason:
        description: The reason why the name is not available.
        type: string
        required: false
  CustomDomain:
    type: Resource
  CustomDomainListResult:
    description: Result of the request to list custom domains. It contains a list of custom domain objects and a URL link to get the next set of results.
    properties:
      nextLink:
        description: URL to get the next set of custom domain objects if there are any.
        type: string
        required: false
      value:
        description: List of CDN CustomDomains within an endpoint.
        items:
          type: CustomDomain
        type: array
        required: false
  CustomDomainParameters:
    description: The customDomain JSON object required for custom domain creation or update.
    properties:
      properties:
        type: CustomDomainPropertiesParameters
        required: false
        (oas-x-ms-client-flatten): true
  CustomDomainProperties:
    description: The JSON object that contains the properties of the custom domain to create.
    properties:
      hostName:
        description: The host name of the custom domain. Must be a domain name.
        type: string
      provisioningState:
        description: Provisioning status of the custom domain.
        readOnly: true
        type: string
        facets:
          readOnly?: boolean
        required: false
      resourceState:
        description: Resource status of the custom domain.
        enum:
          - Creating
          - Active
          - Deleting
        readOnly: true
        type: string
        facets:
          readOnly?: boolean
        required: false
        (oas-x-ms-enum):
          modelAsString: true
          name: CustomDomainResourceState
      validationData:
        description: Special validation or data may be required when delivering CDN to some regions due to local compliance reasons. (e.g. ICP license number of a custom domain is required to deliver content in China.)
        type: string
        required: false
  CustomDomainPropertiesParameters:
    description: The JSON object that contains the properties of the custom domain to create.
    properties:
      hostName:
        description: The host name of the custom domain. Must be a domain name.
        type: string
  DeepCreatedOrigin:
    description: Origins to be added when creating a CDN endpoint.
    properties:
      name:
        description: Origin name
        type: string
      properties:
        type: DeepCreatedOriginProperties
        required: false
        (oas-x-ms-client-flatten): true
  DeepCreatedOriginProperties:
    description: Properties of origins Properties of the origin created on the CDN endpoint.
    properties:
      hostName:
        description: 'The address of the origin. Domain names, IPv4 addresses, and IPv6 addresses are supported.'
        type: string
      httpPort:
        description: The value of the HTTP port. Must be between 1 and 65535
        exclusiveMaximum: false
        exclusiveMinimum: false
        maximum: 65535
        minimum: 1
        type: integer
        facets:
          exclusiveMaximum: boolean
          exclusiveMinimum: boolean
        required: false
      httpsPort:
        description: The value of the HTTPS port. Must be between 1 and 65535
        exclusiveMaximum: false
        exclusiveMinimum: false
        maximum: 65535
        minimum: 1
        type: integer
        facets:
          exclusiveMaximum: boolean
          exclusiveMinimum: boolean
        required: false
  Endpoint:
    type: Resource
  EndpointListResult:
    description: Result of the request to list endpoints. It contains a list of endpoint objects and a URL link to get the the next set of results.
    properties:
      nextLink:
        description: URL to get the next set of endpoint objects if there are any.
        type: string
        required: false
