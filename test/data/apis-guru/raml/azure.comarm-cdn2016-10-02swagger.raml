                          (oas-responses-default):
                            body:
                              application/json:
                                type: ErrorResponse
                            description: CDN error response describing why the operation failed.
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: CustomDomain
                              description: OK. The request has succeeded.
                            '201':
                              body:
                                application/json:
                                  type: CustomDomain
                              description: Created. The request has been fulfilled and a new custom domain has been created.
                            '202':
                              body:
                                application/json:
                                  type: CustomDomain
                              description: Accepted and the operation will complete asynchronously.
                          (oas-tags):
                            - CustomDomains
                    /load:
                      displayName: load
                      post:
                        (oas-x-ms-long-running-operation): true
                        displayName: Endpoints_LoadContent
                        description: Forcibly pre-loads CDN endpoint content. Available for Verizon Profiles.
                        is:
                          - apiVersionParameter
                        body:
                          application/json:
                            type: LoadParameters
                            (oas-body-name): contentFilePaths
