        displayName: '{resourceGroupName}'
        /providers:
          displayName: providers
          /Microsoft.Cdn:
            displayName: Microsoft.Cdn
            /profiles:
              displayName: profiles
              '/{profileName}':
                displayName: '{profileName}'
                /endpoints:
                  displayName: endpoints
                  '/{endpointName}':
                    displayName: '{endpointName}'
                    /customDomains:
                      displayName: customDomains
                      get:
                        (oas-x-ms-pageable):
                          nextLinkName: nextLink
                        displayName: CustomDomains_ListByEndpoint
                        description: Lists the existing CDN custom domains within an endpoint.
                        is:
                          - apiVersionParameter
                        (oas-responses-default):
                          body:
                            application/json:
                              type: ErrorResponse
                          description: CDN error response describing why the operation failed.
                        responses:
                          '200':
                            body:
                              application/json:
                                type: CustomDomainListResult
                            description: OK. The request has succeeded.
                        (oas-tags):
                          - CustomDomains
                      '/{customDomainName}':
                        displayName: '{customDomainName}'
                        uriParameters:
                          customDomainName:
                            description: Name of the custom domain within an endpoint.
                            required: true
                            type: string
                            displayName: Name of the custom domain within an endpoint.
                        delete:
                          (oas-x-ms-long-running-operation): true
                          displayName: CustomDomains_Delete
                          description: Deletes an existing CDN custom domain within an endpoint.
                          is:
                            - apiVersionParameter
                          (oas-responses-default):
                            body:
                              application/json:
                                type: ErrorResponse
                            description: CDN error response describing why the operation failed.
                          responses:
                            '200':
                              description: OK. The request has succeeded.
                            '202':
                              body:
                                application/json:
                                  type: CustomDomain
                              description: Accepted and the operation will complete asynchronously.
                            '204':
                              description: No Content.
                          (oas-tags):
                            - CustomDomains
                        get:
                          displayName: CustomDomains_Get
                          description: Gets an existing CDN custom domain within an endpoint.
                          is:
                            - apiVersionParameter
                          (oas-responses-default):
                            body:
                              application/json:
                                type: ErrorResponse
                            description: CDN error response describing why the operation failed.
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: CustomDomain
                              description: OK. The request has succeeded.
                          (oas-tags):
                            - CustomDomains
                        put:
                          (oas-x-ms-long-running-operation): true
                          displayName: CustomDomains_Create
                          description: Creates a new CDN custom domain within an endpoint.
                          is:
                            - apiVersionParameter
                          body:
