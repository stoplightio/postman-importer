        displayName: getCollectionIconsBySlug
        description: Returns a list of icons associated with a collection
        (oas-summary): Get collection icons by slug
        responses:
          '200':
            description: No response was specified
        queryParameters:
          limit:
            description: Maximum number of results
            format: int32
            required: false
            type: integer
            displayName: Maximum number of results
          offset:
            description: Number of results to displace or skip over
            format: int32
            required: false
            type: integer
            displayName: Number of results to displace or skip over
