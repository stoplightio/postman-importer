/oauth:
  displayName: oauth
  /usage:
    displayName: usage
    get:
      displayName: getApiQuotaStatus
      description: Returns current oauth usage and limits
      (oas-summary): Get api quota status
      responses:
        '200':
          description: No response was specified
      (oas-tags):
        - oauth
/collections:
  displayName: collections
  get:
    displayName: getAllCollections
    description: Return's a list of all collections
    (oas-summary): Get all collections
    responses:
      '200':
        description: No response was specified
    queryParameters:
      limit:
        description: Maximum number of results
        format: int32
        required: false
        type: integer
        displayName: Maximum number of results
      offset:
        description: Number of results to displace or skip over
        format: int32
        required: false
        type: integer
        displayName: Number of results to displace or skip over
      page:
        description: Number of results of limit length to displace or skip over
        format: int32
        required: false
        type: integer
        displayName: Number of results of limit length to displace or skip over
    (oas-tags):
      - collections
(oas-x-hasEquivalentPaths): true
