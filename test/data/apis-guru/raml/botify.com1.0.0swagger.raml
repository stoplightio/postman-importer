                        readOnly: true
                        type: string
                        facets:
                          readOnly?: boolean
                        required: false
                      results:
                        description: ''
                        items:
                          type: Default
                        readOnly: true
                        type: array
                        facets:
                          readOnly?: boolean
                        required: false
                      size:
                        description: ''
                        readOnly: true
                        type: integer
                        facets:
                          readOnly?: boolean
                        required: false
                    type: object
                description: Successful operation
            queryParameters:
              area:
                default: current
                description: Analysis context to execute the query
                enum:
                  - current
                  - disappeared
                  - new
                  - search_engines_orphans
                required: false
                type: string
                displayName: Analysis context to execute the query
              page:
                description: Page Number
                format: int32
                required: false
                type: integer
                displayName: Page Number
              size:
                description: Page Size
                format: int32
                required: false
                type: integer
                displayName: Page Size
