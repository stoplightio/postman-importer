#%RAML 1.0
title: Botify
version: 1.0.0
baseUri: 'https://api.botify.com/v1'
protocols:
  - HTTPS
description: Botify Saas API
(oas-info):
  (oas-x-logo):
    backgroundColor: '#1A1A1A'
    url: 'https://api.apis.guru/v2/cache/logo/https_static.botify.com_images_logos_botify-white.dc4215a12b9c.png'
  (oas-x-origin):
    format: swagger
    url: 'https://api.botify.com/v1/swagger.json'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): botify.com
annotationTypes:
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
  oas-responses-default:
    type: any
    allowedTargets: Method
(oas-externalDocs):
  url: 'https://developers.botify.com/'
securitySchemes:
  DjangoRestToken:
    type: Pass Through
    describedBy:
      headers:
        Authorization:
          type: string
/analyses:
  displayName: analyses
  '/{username}':
    displayName: '{username}'
    '/{project_slug}':
      displayName: '{project_slug}'
      '/{analysis_slug}':
        displayName: '{analysis_slug}'
        /urls:
          displayName: urls
          post:
            displayName: getUrls
            description: Executes a query and returns a paginated response
            (oas-summary): Executes a query and returns a paginated response
            body:
              application/json:
                type: UrlsQuery
                (oas-body-name): UrlsQuery
            (oas-responses-default):
              body:
                application/json:
                  type: DefaultPayload
              description: error payload
            responses:
              '201':
                body:
                  application/json:
                    properties:
                      count:
                        description: ''
                        readOnly: true
                        type: integer
                        facets:
                          readOnly?: boolean
                        required: false
                      next:
                        description: ''
                        readOnly: true
                        type: string
                        facets:
                          readOnly?: boolean
                        required: false
                      page:
                        description: ''
                        readOnly: true
                        type: integer
                        facets:
                          readOnly?: boolean
                        required: false
                      previous:
