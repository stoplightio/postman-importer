#%RAML 1.0
title: GoToWebinar
version: 1.0.0
baseUri: 'https://api.citrixonline.com/G2W/rest'
mediaType: application/json
protocols:
  - HTTPS
description: 'The Citrix GoToWebinar API provides seamless integration of webinar registrant and attendee data into your existing infrastructure or third-party applications. The ability to register participants, as well as pull lists of registrants and attendees for a webinar, allows organizers to manage the flow of information between their primary applications without manual intervention.'
(oas-tags-definition):
  - description: Operations available for webinars of a given organizer.
    name: Webinars
  - description: Operations available for co-organizers of a given webinar.
    name: Co-organizers
  - description: Operations available for panelists of a given webinar.
    name: Panelists
  - description: Operations available for registrants of a given webinar.
    name: Registrants
  - description: Operations available for sessions of a given webinar.
    name: Sessions
  - description: Operations available for attendees of a given webinar session.
    name: Attendees
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-logo):
    backgroundColor: '#5F6166'
    url: 'https://api.apis.guru/v2/cache/logo/https_developer.citrixonline.com_sites_all_themes_citrix_responsive_logo.png'
  (oas-x-origin):
    format: swagger
    url: 'https://developer.citrixonline.com/sites/default/files/citrix/citrix-apis/gotowebinar.json'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): citrixonline.com
  (oas-x-serviceName): gotowebinar
  contact:
    name: Developer Support
    url: 'https://developer.citrixonline.com'
    email: developer-support@citrixonline.com
  termsOfService: 'https://developer.citrixonline.com/terms-use'
(oas-externalDocs):
  url: 'https://developer.citrixonline.com/'
/organizers:
  displayName: organizers
  '/{organizerKey}':
    displayName: '{organizerKey}'
    /webinars:
      displayName: webinars
      '/{webinarKey}':
        displayName: '{webinarKey}'
        /panelists:
          displayName: panelists
          '/{panelistKey}':
            displayName: '{panelistKey}'
            uriParameters:
              panelistKey:
                description: The key of the webinar panelist
                format: int64
                required: true
                type: integer
                displayName: The key of the webinar panelist
            delete:
              displayName: deleteWebinarPanelist
              description: Removes a webinar panelist.
              (oas-summary): Delete webinar panelist
              is:
                - authorization
              responses:
                '204':
                  description: No content
                '400':
                  description: Bad Request
                '403':
                  description: Forbidden
                '404':
                  description: Not found
              (oas-tags):
                - Panelists
            /resendInvitation:
              displayName: resendInvitation
              post:
                displayName: resendPanelistInvitation
                description: Resend the panelist invitation email.
                (oas-summary): Resend panelist invitation
                is:
                  - authorization
                responses:
                  '204':
                    description: Created
                  '400':
                    description: Bad Request
                  '403':
                    description: Forbidden
                  '404':
                    description: Not Found
                (oas-tags):
                  - Panelists
          get:
            displayName: getPanelists
            description: Retrieves all the panelists for a specific webinar.
            (oas-summary): Get webinar panelists
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    items:
                      type: Panelist
                    type: array
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
            (oas-tags):
              - Panelists
          post:
            displayName: createPanelists
            description: Create panelists for a specified webinar
            (oas-summary): Create Panelists
            is:
              - authorization
            body:
              application/json:
                items:
                  type: PanelistReqCreate
                type: array
                (oas-body-name): body
                description: Array of panelists
            responses:
              '201':
                body:
                  application/json:
                    items:
                      type: CreatedPanelist
                    type: array
                description: Created
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not Found
            (oas-tags):
              - Panelists
        /sessions:
          displayName: sessions
          '/{sessionKey}':
            displayName: '{sessionKey}'
            /surveys:
              displayName: surveys
              get:
                displayName: getSurveys
                description: Retrieve surveys for a past webinar session.
                (oas-summary): Get session surveys
                is:
                  - authorization
                responses:
                  '200':
                    body:
                      application/json:
                        items:
                          type: Poll
                        type: array
                    description: OK
                  '400':
                    description: Bad Request
                  '403':
                    description: Forbidden
                  '404':
                    description: Not Found
                (oas-tags):
                  - Sessions
            uriParameters:
              sessionKey:
                description: The key of the webinar session
                format: int64
                required: true
                type: integer
                displayName: The key of the webinar session
            get:
              displayName: getWebinarSession
              description: 'Retrieves attendance details for a specific webinar session that has ended. If attendees attended the session (''registrantsAttended''), specific attendance details, such as attendenceTime for a registrant, will also be retrieved.'
              (oas-summary): Get webinar session
              is:
                - authorization
              responses:
                '200':
                  body:
                    application/json:
                      items:
                        type: Attendee
                      type: array
                  description: OK
                '400':
                  description: Bad Request
                '403':
                  description: Forbidden
                '404':
                  description: Not Found
              (oas-tags):
                - Sessions
            /attendees:
              displayName: attendees
              get:
                displayName: getAttendees
                description: Retrieve details for all attendees of a specific webinar session.
                (oas-summary): Get session attendees
                is:
                  - authorization
                responses:
                  '200':
                    body:
                      application/json:
                        items:
                          type: Attendee
                        type: array
                    description: OK
                  '400':
                    description: Bad Request
                  '403':
                    description: Forbidden
                  '404':
                    description: Not found
                (oas-tags):
                  - Attendees
              '/{registrantKey}':
                displayName: '{registrantKey}'
                uriParameters:
                  registrantKey:
                    description: The key of the registrant
                    format: int64
                    required: true
                    type: integer
                    displayName: The key of the registrant
                get:
                  displayName: getAttendee
                  description: Retrieve registration details for a particular attendee of a specific webinar session.
                  (oas-summary): Get attendee
                  is:
                    - authorization
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Registrant
                      description: OK
                    '400':
                      description: Bad Request
                    '403':
                      description: Forbidden
                    '404':
                      description: Not found
                  (oas-tags):
                    - Attendees
                /polls:
                  displayName: polls
                  get:
                    displayName: getAttendeePollAnswers
                    description: Get poll answers from a particular attendee of a specific webinar session.
                    (oas-summary): Get attendee poll answers
                    is:
                      - authorization
                    responses:
                      '200':
                        body:
                          application/json:
                            items:
                              type: PollAnswer
                            type: array
                        description: OK
                      '400':
                        description: Bad Request
                      '403':
                        description: Forbidden
                      '404':
                        description: Not found
                    (oas-tags):
                      - Attendees
                /questions:
                  displayName: questions
                  get:
                    displayName: getAttendeeQuestions
                    description: Get questions asked by an attendee during a webinar session.
                    (oas-summary): Get attendee questions
                    is:
                      - authorization
                    responses:
                      '200':
                        body:
                          application/json:
                            items:
                              type: AttendeeQuestion
                            type: array
                        description: OK
                      '400':
                        description: Bad Request
                      '403':
                        description: Forbidden
                      '404':
                        description: Not found
                    (oas-tags):
                      - Attendees
                /surveys:
                  displayName: surveys
                  get:
                    displayName: getAttendeeSurveyAnswers
                    description: Retrieve survey answers from a particular attendee during a webinar session.
                    (oas-summary): Get attendee survey answers
                    is:
                      - authorization
                    responses:
                      '200':
                        body:
                          application/json:
                            items:
                              type: PollAnswer
                            type: array
                        description: OK
                      '400':
                        description: Bad Request
                      '403':
                        description: Forbidden
                      '404':
                        description: Not found
                    (oas-tags):
                      - Attendees
            /questions:
              displayName: questions
              get:
                displayName: getQuestions
                description: Retrieve questions and answers for a past webinar session.
                (oas-summary): Get session questions
                is:
                  - authorization
                responses:
                  '200':
                    body:
                      application/json:
                        items:
                          type: AttendeeQuestion
                        type: array
                    description: OK
                  '400':
                    description: Bad Request
                  '403':
                    description: Forbidden
                  '404':
                    description: Not Found
                (oas-tags):
                  - Sessions
            /polls:
              displayName: polls
              get:
                displayName: getPolls
                description: Retrieve all collated attendee questions and answers for polls from a specific webinar session.
                (oas-summary): Get session polls
                is:
                  - authorization
                responses:
                  '200':
                    body:
                      application/json:
                        items:
                          type: Poll
                        type: array
                    description: OK
                  '400':
                    description: Bad Request
                  '403':
                    description: Forbidden
                  '404':
                    description: Not Found
                (oas-tags):
                  - Sessions
            /performance:
              displayName: performance
              get:
                displayName: getPerformance
                description: Get performance details for a session.
                (oas-summary): Get session performance
                is:
                  - authorization
                responses:
                  '200':
                    body:
                      application/json:
                        type: SessionPerformance
                    description: OK
                  '400':
                    description: Bad Request
                  '403':
                    description: Forbidden
                  '404':
                    description: Not Found
                (oas-tags):
                  - Sessions
          get:
            displayName: getAllSessions
            description: Retrieves details for all past sessions of a specific webinar.
            (oas-summary): Get webinar sessions
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    items:
                      type: Session
                    type: array
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not Found
            (oas-tags):
              - Sessions
        /performance:
          displayName: performance
          get:
            displayName: getPerformanceForAllWebinarSessions
            description: Gets performance details for all sessions of a specific webinar.
            (oas-summary): Get performance for all webinar sessions
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    additionalProperties:
                      type: SessionPerformance
                    description: Describes performance details for webinars
                    type: object
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
            (oas-tags):
              - Webinars
        /registrants:
          displayName: registrants
          get:
            displayName: getAllRegistrantsForWebinar
            description: 'Retrieve registration details for all registrants of a specific webinar. Registrant details will not include all fields captured when creating the registrant. To see all data, use the API call ''Get Registrant''. Registrants can have one of the following states; <br>WAITING - registrant registered and is awaiting approval (where organizer has required approval), <br>APPROVED - registrant registered and is approved, and <br>DENIED - registrant registered and was denied.'
            (oas-summary): Get registrants
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    items:
                      type: Registrant
                    type: array
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
            (oas-tags):
              - Registrants
          post:
            displayName: createRegistrant
            description: 'Register an attendee for a scheduled webinar. The response contains the registrantKey and join URL for the registrant. An email will be sent to the registrant unless the organizer turns off the confirmation email setting from the GoToWebinar website. Please note that you must provide all required fields including custom fields defined during the webinar creation. Use the API call ''Get registration fields'' to get a list of all fields, if they are required, and their possible values. At this time there are two versions of the ''Create Registrant'' call. The first version only accepts firstName, lastName, and email and ignores all other fields. If you have custom fields or want to capture additional information this version won''t work for you. The second version allows you to pass all required and optional fields, including custom fields defined when creating the webinar. To use the second version you must pass the header value ''Accept: application/vnd.citrix.g2wapi-v1.1+json'' instead of ''Accept: application/json''. Leaving this header out results in the first version of the API call.'
            (oas-summary): Create registrant
            is:
              - authorization
            body:
              application/json:
                type: RegistrantFields
                (oas-body-name): body
                description: The registrant details. To get all possible values run the API call 'Get registration fields' which is also part of this documentation.
            headers:
              Accept:
                type: string
                description: Set to 'application/vnd.citrix.g2wapi-v1.1+json' to make a registration using fields (custom or default) additional to the basic ones.
                required: false
            responses:
              '201':
                body:
                  application/json:
                    type: RegistrantCreated
                description: Created
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
              '409':
                description: The user is already registered
            queryParameters:
              resendConfirmation:
                description: Indicates whether the confirmation email should be resent when a registrant is re-registered. The default value is false.
                required: false
                type: boolean
                displayName: Indicates whether the confirmation email should be resent when a registrant is re-registered. The default value is false.
            (oas-tags):
              - Registrants
          /fields:
            displayName: fields
            get:
              displayName: getRegistrationFields
              description: 'Retrieve required, optional registration, and custom questions for a specified webinar.'
              (oas-summary): Get registration fields
              is:
                - authorization
              responses:
                '200':
                  body:
                    application/json:
                      type: RegistrationFields
                  description: OK
                '400':
                  description: Bad Request
                '403':
                  description: Forbidden
                '404':
                  description: Not found
              (oas-tags):
                - Registrants
          '/{registrantKey}':
            displayName: '{registrantKey}'
            uriParameters:
              registrantKey:
                description: The key of the registrant
                format: int64
                required: true
                type: integer
                displayName: The key of the registrant
            delete:
              displayName: deleteRegistrant
              description: 'Removes a webinar registrant from current registrations for the specified webinar. The webinar must be a scheduled, future webinar.'
              (oas-summary): Delete registrant
              is:
                - authorization
              responses:
                '204':
                  description: No content
                '400':
                  description: Bad Request
                '403':
                  description: Forbidden
                '404':
                  description: Not found
              (oas-tags):
                - Registrants
            get:
              displayName: getRegistrant
              description: Retrieve registration details for a specific registrant.
              (oas-summary): Get registrant
              is:
                - authorization
              responses:
                '200':
                  body:
                    application/json:
                      type: RegistrantDetailed
                  description: OK
                '400':
                  description: Bad Request
                '403':
                  description: Forbidden
                '404':
                  description: Not found
              (oas-tags):
                - Registrants
        uriParameters:
          webinarKey:
            description: The key of the webinar
            format: int64
            required: true
            type: integer
            displayName: The key of the webinar
        get:
          displayName: getWebinar
          description: 'Retrieve information on a specific webinar. If the type of the webinar is ''sequence'', a sequence of future times will be provided. Webinars of type ''series'' are treated the same as normal webinars - each session in the webinar series has a different webinarKey. If an organizer cancels a webinar, then a request to get that webinar would return a ''404 Not Found'' error.'
          (oas-summary): Get webinar
          is:
            - authorization
          responses:
            '200':
              body:
                application/json:
                  type: WebinarByKey
              description: OK
            '400':
              description: Bad Request
            '403':
              description: Forbidden
            '404':
              description: Not found
          (oas-tags):
            - Webinars
        /attendees:
          displayName: attendees
          get:
            displayName: getAttendeesForAllWebinarSessions
            description: Returns all attendees for all sessions of the specified webinar.
            (oas-summary): Get attendees for all webinar sessions
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    items:
                      type: Attendee
                    type: array
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
            (oas-tags):
              - Webinars
        /audio:
          displayName: audio
          post:
            displayName: updateAudioInformation
            description: Updates the audio/conferencing settings for a specific webinar
            (oas-summary): Update audio information
            is:
              - authorization
            body:
              application/json:
                type: AudioUpdate
                (oas-body-name): body
                description: The audio/conferencing settings
            responses:
              '204':
                description: No Content
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
            queryParameters:
              notifyParticipants:
                description: Defines whether to send notifications to participants
                required: false
                type: boolean
                displayName: Defines whether to send notifications to participants
            (oas-tags):
              - Webinars
          get:
            displayName: getAudioInformation
            description: Retrieves the audio/conferencing information for a specific webinar.
            (oas-summary): Get audio information
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    type: Audio
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
            (oas-tags):
              - Webinars
        /coorganizers:
          displayName: coorganizers
          post:
            displayName: createCoorganizers
            description: Creates co-organizers for the specified webinar. This call requires the admin role. For co-organizers that have a GoToWebinar account you have to set the parameter 'external' to 'false'. In this case you have to pass the parameter 'organizerKey' only. For co-organizers that have no GoToWebinar account you have to set the parameter 'external' to 'true'. In this case you have to pass the parameters 'givenName' and 'email'. Since there is no parameter for 'surname' you should pass first and last name to the parameter 'givenName'.
            (oas-summary): Create co-organizers
            is:
              - authorization
            body:
              application/json:
                items:
                  type: CoorganizerReqCreate
                type: array
                (oas-body-name): body
                description: The co-organizer details
            responses:
              '201':
                body:
                  application/json:
                    items:
                      type: Coorganizer
                    type: array
                description: Created
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not Found
            (oas-tags):
              - Co-organizers
          '/{coorganizerKey}':
            displayName: '{coorganizerKey}'
            /resendInvitation:
              displayName: resendInvitation
              post:
                displayName: resendCoorganizerInvitation
                description: Resends an invitation email to the specified co-organizer
                (oas-summary): Resend invitation
                is:
                  - authorization
                responses:
                  '204':
                    description: No Content (Invitation email was sent)
                  '400':
                    description: Bad Request
                  '403':
                    description: Forbidden
                  '404':
                    description: Not Found
                queryParameters:
                  external:
                    description: By default only internal co-organizers (with a GoToWebinar account) will retrieve the resent invitation email. If you want to use this call for external co-organizers you have to set this parameter to 'true'.
                    required: false
                    type: boolean
                    displayName: By default only internal co-organizers (with a GoToWebinar account) will retrieve the resent invitation email. If you want to use this call for external co-organizers you have to set this parameter to 'true'.
                (oas-tags):
                  - Co-organizers
            uriParameters:
              organizerKey:
                description: The key of the organizer
                format: int64
                required: true
                type: integer
                displayName: The key of the organizer
              coorganizerKey:
                description: The key of the internal or external co-organizer (memberKey)
                format: int64
                required: true
                type: integer
                displayName: The key of the internal or external co-organizer (memberKey)
            delete:
              displayName: deleteCoorganizer
              description: Deletes an internal co-organizer specified by the coorganizerKey (memberKey).
              (oas-summary): Delete co-organizer
              is:
                - authorization
              responses:
                '204':
                  description: No Content (Co-organizer was deleted)
                '400':
                  description: Bad Request
                '403':
                  description: Forbidden
                '404':
                  description: Not Found
              queryParameters:
                external:
                  description: By default only internal co-organizers (with a GoToWebinar account) can be deleted. If you want to use this call for external co-organizers you have to set this parameter to 'true'.
                  required: false
                  type: boolean
                  displayName: By default only internal co-organizers (with a GoToWebinar account) can be deleted. If you want to use this call for external co-organizers you have to set this parameter to 'true'.
              (oas-tags):
                - Co-organizers
          get:
            displayName: getCoorganizers
            description: 'Returns the co-organizers for the specified webinar. The original organizer who created the webinar is filtered out of the list. If the webinar has no co-organizers, an empty array is returned. Co-organizers that do not have a GoToWebinar account are returned as external co-organizers. For those organizers no surname is returned.'
            (oas-summary): Get co-organizers
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    items:
                      type: Coorganizer
                    type: array
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not Found
            (oas-tags):
              - Co-organizers
        delete:
          displayName: cancelWebinar
          description: 'Cancels a specific webinar. If the webinar is a series or sequence, this call deletes all scheduled sessions. To send cancellation emails to registrants set sendCancellationEmails=true in the request. When the cancellation emails are sent, the default generated message is used in the cancellation email body.'
          (oas-summary): Cancel webinar
          is:
            - authorization
          responses:
            '204':
              description: No Content
            '403':
              description: Forbidden
            '404':
              description: Not found
            '405':
              description: Method Not Allowed (Webinar is in the past)
            '409':
              description: Conflict (Webinar is in session)
          queryParameters:
            sendCancellationEmails:
              description: Indicates whether cancellation notice emails should be sent. The default value is false
              required: false
              type: boolean
              displayName: Indicates whether cancellation notice emails should be sent. The default value is false
          (oas-tags):
            - Webinars
        /meetingtimes:
          displayName: meetingtimes
          get:
            displayName: getWebinarMeetingTimes
            description: Retrieves the meeting times for a webinar.
            (oas-summary): Get webinar meeting times
            is:
              - authorization
            responses:
              '200':
                body:
                  application/json:
                    items:
                      type: DateTimeRange
                    type: array
                description: OK
              '400':
                description: Bad Request
              '403':
                description: Forbidden
              '404':
                description: Not found
            (oas-tags):
              - Webinars
        put:
          displayName: updateWebinar
          description: 'Updates a webinar. The call requires at least one of the parameters in the request body. The request completely replaces the existing session, series, or sequence and so must include the full definition of each as for the Create call. Set notifyParticipants=true to send update emails to registrants.'
          (oas-summary): Update webinar
          is:
            - authorization
          body:
            application/json:
              type: WebinarReqUpdate
              (oas-body-name): body
              description: The webinar details
          responses:
            '202':
              description: Accepted
            '400':
