        is:
          - authorization
        body:
          application/json:
            type: WebinarReqCreate
            (oas-body-name): body
            description: The webinar details
        responses:
          '201':
            body:
              application/json:
                type: CreatedWebinar
            description: Created
          '400':
            description: Bad Request
          '403':
            description: Forbidden
