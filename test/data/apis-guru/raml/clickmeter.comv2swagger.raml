#%RAML 1.0
title: ClickMeter
version: v2
baseUri: 'http://apiv2.clickmeter.com:80'
protocols:
  - HTTP
  - HTTPS
description: Api dashboard for ClickMeter API
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_s3.amazonaws.com_clickmeter.com_Web_static_cmlogo.svg'
  (oas-x-origin):
    format: swagger
    url: 'http://api.v2.clickmeter.com.s3.amazonaws.com/docs/api-docs-v2.json'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): clickmeter.com
  contact:
    name: Api Support
    url: 'http://www.clickmeter.com/api'
    email: api@clickmeter.com
annotationTypes:
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
  oas-definition-name:
    type: string
    allowedTargets: TypeDeclaration
securitySchemes:
  api_key:
    type: Pass Through
    describedBy:
      headers:
        X-Clickmeter-AuthKey:
          type: string
    description: API Key Authentication
/datapoints:
  displayName: datapoints
  '/{id}':
    displayName: '{id}'
    /favourite:
      displayName: favourite
      put:
        displayName: PUT_datapoints-id-favourite
        (oas-summary): Fast switch the "favourite" field of a datapoint
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Responses.EntityUri_System.Int64_
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
          '404':
            description: Not found
          '500':
            description: Internal Server Error
        (oas-tags):
          - DataPoints
    /hits:
      displayName: hits
      get:
        displayName: GET_datapoints-id-hits
        (oas-summary): Retrieve the list of events related to this datapoint.
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Dto.ClickStream.HitListPage
          '401':
            description: Unauthorized
          '500':
            description: Internal Server Error
        queryParameters:
          timeframe:
            description: Timeframe of the request. See list at $timeframeList
            enum:
              - yesterday
              - last7
              - last30
              - lastmonth
              - currentmonth
              - previousmonth
              - last90
              - last120
              - last180
              - custom
            required: true
            type: string
            displayName: Timeframe of the request. See list at $timeframeList
          limit:
            description: Limit results to this number
            format: int32
            required: false
            type: integer
            displayName: Limit results to this number
          offset:
            description: Offset where to start from (it's the lastKey field in the response object)
            required: false
            type: string
            displayName: Offset where to start from (it's the lastKey field in the response object)
          fromDay:
            description: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
          toDay:
            description: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
          filter:
            description: Filter event type ("spiders"/"uniques"/"nonuniques"/"conversions")
            enum:
              - spiders
              - uniques
              - nonuniques
              - conversions
            required: false
            type: string
            displayName: Filter event type ("spiders"/"uniques"/"nonuniques"/"conversions")
        (oas-tags):
          - DataPoints
    /notes:
      displayName: notes
      put:
        displayName: PUT_datapoints-id-notes
        (oas-summary): Fast patch the "notes" field of a datapoint
        body:
          application/json:
            type: Api.Core.Requests.GenericTextPatch
            (oas-body-name): note
            description: Patch requests
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Responses.EntityUri_System.Int64_
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
          '404':
            description: Not found
          '500':
            description: Internal Server Error
        (oas-tags):
          - DataPoints
    /reports:
      displayName: reports
      get:
        displayName: GET_datapoints-id-reports
        (oas-summary): Retrieve a top report connected to this datapoint
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Dto.Tops.Top
          '401':
            description: Forbidden
          '404':
            description: Not found
          '500':
            description: Internal Server Error
        queryParameters:
          type:
            description: Type of the report.
            enum:
              - browsers
              - browsersfamilies
              - platforms
              - cities
              - countries
              - isps
              - ips
              - oss
              - ossfamilies
              - keywords
              - referrers
              - destinations
              - languages
              - params
            required: true
            type: string
            displayName: Type of the report.
          timeframe:
            description: Timeframe of the request. See list at $timeframeList
            enum:
              - yesterday
              - last7
              - last30
              - lastmonth
              - currentmonth
              - previousmonth
              - last90
              - last120
              - last180
              - beginning
              - custom
            required: true
            type: string
            displayName: Timeframe of the request. See list at $timeframeList
          fromDay:
            description: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
          toDay:
            description: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
        (oas-tags):
          - DataPoints
    uriParameters:
      id:
        description: The id of the datapoint
        format: int64
        required: true
        type: integer
        displayName: The id of the datapoint
    get:
      displayName: GET_datapoints-id
      (oas-summary): Get a datapoint
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Dto.Datapoints.Datapoint
        '401':
          description: Unauthorized
        '404':
          description: Not found
        '500':
          description: Internal Server Error
      (oas-tags):
        - DataPoints
    /aggregated:
      displayName: aggregated
      get:
        displayName: GET_datapoints-id-aggregated
        (oas-summary): Retrieve statistics about this datapoint for a timeframe
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Dto.Aggregated.AggregatedResult
          '401':
            description: Unauthorized
          '500':
            description: Internal Server Error
        queryParameters:
          timeFrame:
            description: Timeframe of the request. See list at $timeframeList
            enum:
              - today
              - yesterday
              - last7
              - last30
              - lastmonth
              - currentmonth
              - previousmonth
              - last90
              - last120
              - last180
              - last12months
              - lastyear
              - currentyear
              - beginning
              - custom
            required: true
            type: string
            displayName: Timeframe of the request. See list at $timeframeList
          fromDay:
            description: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
          toDay:
            description: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
          hourly:
            description: If using "yesterday" or "today" timeframe you can ask for the hourly detail
            required: false
            type: boolean
            displayName: If using "yesterday" or "today" timeframe you can ask for the hourly detail
        (oas-tags):
          - DataPoints
      /list:
        displayName: list
        get:
          displayName: GET_datapoints-id-aggregated-list
          (oas-summary): Retrieve statistics about this datapoint for a timeframe grouped by some temporal entity (day/week/month)
          responses:
            '200':
              body:
                application/json:
                  type: Api.Core.Responses.EntitiesResponse_Api.Core.Dto.Aggregated.AggregatedResult_
            '401':
              description: Unauthorized
            '500':
              description: Internal Server Error
          queryParameters:
            timeFrame:
              description: Timeframe of the request. See list at $timeframeList
              enum:
                - today
                - yesterday
                - last7
                - last30
                - lastmonth
                - currentmonth
                - previousmonth
                - last90
                - last120
                - last180
                - last12months
                - lastyear
                - currentyear
                - beginning
                - custom
              required: true
              type: string
              displayName: Timeframe of the request. See list at $timeframeList
            fromDay:
              description: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
              required: false
              type: string
              displayName: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
            toDay:
              description: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
              required: false
              type: string
              displayName: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
            groupBy:
              description: The temporal entity you want to group by ("week"/"month"). If unspecified is "day".
              enum:
                - week
                - month
              required: false
              type: string
              displayName: The temporal entity you want to group by ("week"/"month"). If unspecified is "day".
          (oas-tags):
            - DataPoints
    delete:
      displayName: DELETE_datapoints-id
      (oas-summary): Delete a datapoint
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Responses.EntityUri_System.Int64_
        '401':
          description: Unauthorized
        '404':
          description: Not found
        '500':
          description: Internal Server Error
      (oas-tags):
        - DataPoints
    post:
      displayName: POST_datapoints-id
      (oas-summary): Update a datapoint
      body:
        application/json:
          type: Api.Core.Dto.Datapoints.Datapoint
          (oas-body-name): value
          description: The body of the datapoint
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Responses.EntityUri_System.Int64_
        '401':
          description: Unauthorized
        '404':
          description: Not found
        '500':
          description: Internal Server Error
      (oas-tags):
        - DataPoints
  post:
    displayName: POST_datapoints
    (oas-summary): Create a datapoint
    body:
      application/json:
        type: Api.Core.Dto.Datapoints.Datapoint
        (oas-body-name): value
        description: The body of the datapoint
    responses:
      '200':
        body:
          application/json:
            type: Api.Core.Responses.EntityUri_System.Int64_
      '401':
        description: Unauthorized
      '404':
        description: Not found
      '500':
        description: Internal Server Error
    (oas-tags):
      - DataPoints
  /aggregated:
    displayName: aggregated
    /list:
      displayName: list
      get:
        displayName: GET_datapoints-aggregated-list
        (oas-summary): Retrieve statistics about all datapoints of this customer for a timeframe grouped by some temporal entity (day/week/month)
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Responses.EntitiesResponse_Api.Core.Dto.Aggregated.AggregatedResult_
          '401':
            description: Unauthorized
          '500':
            description: Internal Server Error
        queryParameters:
          type:
            description: Type of datapoint ("tl"/"tp")
            enum:
              - tp
              - tl
            required: true
            type: string
            displayName: Type of datapoint ("tl"/"tp")
          timeFrame:
            description: Timeframe of the request. See list at $timeframeList
            enum:
              - today
              - yesterday
              - last7
              - last30
              - lastmonth
              - currentmonth
              - previousmonth
              - last90
              - last120
              - last180
              - last12months
              - lastyear
              - currentyear
              - beginning
              - custom
            required: true
            type: string
            displayName: Timeframe of the request. See list at $timeframeList
          fromDay:
            description: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
          toDay:
            description: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
            required: false
            type: string
            displayName: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
          status:
            description: Status of datapoint ("deleted"/"active"/"paused"/"spam")
            enum:
              - deleted
              - active
              - paused
              - spam
            required: false
            type: string
            displayName: Status of datapoint ("deleted"/"active"/"paused"/"spam")
          tag:
            description: A comma separated list of tags you want to filter with.
            required: false
            type: string
            displayName: A comma separated list of tags you want to filter with.
          favourite:
            description: Is the datapoint is marked as favourite
            required: false
            type: boolean
            displayName: Is the datapoint is marked as favourite
          groupBy:
            description: The temporal entity you want to group by ("week"/"month"). If unspecified is "day".
            enum:
              - week
              - month
            required: false
            type: string
            displayName: The temporal entity you want to group by ("week"/"month"). If unspecified is "day".
        (oas-tags):
          - DataPoints
    get:
      displayName: GET_datapoints-aggregated
      (oas-summary): Retrieve statistics about this customer for a timeframe by groups
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Dto.Aggregated.AggregatedResult
        '401':
          description: Unauthorized
        '500':
          description: Internal Server Error
      queryParameters:
        timeFrame:
          description: Timeframe of the request. See list at $timeframeList
          enum:
            - today
            - yesterday
            - last7
            - last30
            - lastmonth
            - currentmonth
            - previousmonth
            - last90
            - last120
            - last180
            - last12months
            - lastyear
            - currentyear
            - beginning
            - custom
          required: true
          type: string
          displayName: Timeframe of the request. See list at $timeframeList
        type:
          description: Type of datapoint ("tl"/"tp")
          enum:
            - tp
            - tl
          required: false
          type: string
          displayName: Type of datapoint ("tl"/"tp")
        fromDay:
          description: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
          required: false
          type: string
          displayName: If using a "custom" timeFrame you can specify the starting day (YYYYMMDD)
        toDay:
          description: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
          required: false
          type: string
          displayName: If using a "custom" timeFrame you can specify the ending day (YYYYMMDD)
        hourly:
          description: If using "yesterday" or "today" timeframe you can ask for the hourly detail
          required: false
          type: boolean
          displayName: If using "yesterday" or "today" timeframe you can ask for the hourly detail
        status:
          description: Status of datapoint ("deleted"/"active"/"paused"/"spam")
          enum:
            - deleted
            - active
            - paused
            - spam
          required: false
          type: string
          displayName: Status of datapoint ("deleted"/"active"/"paused"/"spam")
        tag:
          description: A comma separated list of tags you want to filter with.
          required: false
          type: string
          displayName: A comma separated list of tags you want to filter with.
        favourite:
          description: Is the datapoint is marked as favourite
          required: false
          type: boolean
          displayName: Is the datapoint is marked as favourite
      (oas-tags):
        - DataPoints
  /batch:
    displayName: batch
    post:
      displayName: POST_datapoints-batch
      (oas-summary): Update multiple datapoints
      body:
        application/json:
          type: Api.Core.Requests.DatapointsBatch
          (oas-body-name): batch
          description: A json containing the datapoints to update.
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Responses.ModifyBatchItemResponce_Api.Core.Dto.Datapoints.Datapoint_System.Int64_
        '401':
          description: Unauthorized
        '404':
          description: Not found
        '500':
          description: Internal Server Error
      (oas-tags):
        - DataPoints
    delete:
      displayName: DELETE_datapoints-batch
      (oas-summary): Delete multiple datapoints
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Responses.ModifyBatchItemResponce_Api.Core.Dto.Datapoints.Datapoint_System.Int64_
        '401':
          description: Unauthorized
        '404':
          description: Not found
        '500':
          description: Internal Server Error
      (oas-tags):
        - DataPoints
    put:
      displayName: PUT_datapoints-batch
      (oas-summary): Create multiple datapoints
      body:
        application/json:
          type: Api.Core.Requests.DatapointsBatch
          (oas-body-name): batch
          description: A json containing the datapoints to create.
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Responses.ModifyBatchItemResponce_Api.Core.Dto.Datapoints.Datapoint_System.Int64_
        '401':
          description: Unauthorized
        '404':
          description: Not found
        '500':
          description: Internal Server Error
      (oas-tags):
        - DataPoints
  /count:
    displayName: count
    get:
      displayName: GET_datapoints-count
      (oas-summary): Count the datapoints associated to the user
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Responses.CountResponce
        '401':
          description: Unauthorized
        '404':
          description: Not found
        '500':
          description: Internal Server Error
      queryParameters:
        type:
          description: Type of the datapoint ("tp"/"tl")
          enum:
            - tp
            - tl
          required: false
          type: string
          displayName: Type of the datapoint ("tp"/"tl")
        status:
          description: Status of the datapoint
          enum:
            - deleted
            - active
            - paused
            - spam
          required: false
          type: string
          displayName: Status of the datapoint
        tags:
          description: A comma separated list of tags you want to filter with.
          required: false
          type: string
          displayName: A comma separated list of tags you want to filter with.
        textSearch:
          description: Filter fields by this pattern
          required: false
          type: string
          displayName: Filter fields by this pattern
        onlyFavorites:
          description: Filter fields by favourite status
          required: false
          type: boolean
          displayName: Filter fields by favourite status
        createdAfter:
          description: Exclude datapoints created before this date (YYYYMMDD)
          required: false
          type: string
          displayName: Exclude datapoints created before this date (YYYYMMDD)
        createdBefore:
          description: Exclude datapoints created after this date (YYYYMMDD)
          required: false
          type: string
          displayName: Exclude datapoints created after this date (YYYYMMDD)
      (oas-tags):
        - DataPoints
  get:
    displayName: GET_datapoints
    (oas-summary): List of all the datapoints associated to the user
    responses:
      '200':
        body:
          application/json:
            type: Api.Core.Responses.EntitiesResponse_Api.Core.Responses.EntityUri_System.Int64__
      '401':
        description: Unauthorized
      '404':
        description: Not found
      '500':
        description: Internal Server Error
    queryParameters:
      offset:
        default: 0
        description: Where to start when retrieving elements. Default is 0 if not specified.
        format: int32
        required: false
        type: integer
        displayName: Where to start when retrieving elements. Default is 0 if not specified.
      limit:
        default: 20
        description: Maximum elements to retrieve. Default to 20 if not specified.
        format: int32
        required: false
        type: integer
        displayName: Maximum elements to retrieve. Default to 20 if not specified.
      type:
        description: Type of the datapoint ("tp"/"tl")
        enum:
          - tp
          - tl
        required: false
        type: string
        displayName: Type of the datapoint ("tp"/"tl")
      status:
        description: Status of the datapoint
        enum:
          - deleted
          - active
          - paused
          - spam
        required: false
        type: string
        displayName: Status of the datapoint
      tags:
        description: A comma separated list of tags you want to filter with.
        required: false
        type: string
        displayName: A comma separated list of tags you want to filter with.
      textSearch:
        description: Filter fields by this pattern
        required: false
        type: string
        displayName: Filter fields by this pattern
      onlyFavorites:
        description: Filter fields by favourite status
        required: false
        type: boolean
        displayName: Filter fields by favourite status
      sortBy:
        description: Field to sort by
        required: false
        type: string
        displayName: Field to sort by
      sortDirection:
        description: Direction of sort "asc" or "desc"
        enum:
          - asc
          - desc
        required: false
        type: string
        displayName: Direction of sort "asc" or "desc"
      createdAfter:
        description: Exclude datapoints created before this date (YYYYMMDD)
        required: false
        type: string
        displayName: Exclude datapoints created before this date (YYYYMMDD)
      createdBefore:
        description: Exclude datapoints created after this date (YYYYMMDD)
        required: false
        type: string
        displayName: Exclude datapoints created after this date (YYYYMMDD)
    (oas-tags):
      - DataPoints
/tags:
  displayName: tags
  '/{tagId}':
    displayName: '{tagId}'
    /name:
      displayName: name
      put:
        displayName: PUT_tags-tagId-name
        (oas-summary): Fast patch a tag name
        body:
          application/json:
            type: Api.Core.Requests.GenericTextPatch
            (oas-body-name): data
            description: The body patch
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Responses.EntityUri_System.Int64_
          '401':
            description: Unauthorized
          '500':
            description: Internal Server Error
        (oas-tags):
          - Tags
    uriParameters:
      tagId:
        description: Id of the tag
        format: int64
        required: true
        type: integer
        displayName: Id of the tag
    delete:
      displayName: DELETE_tags-tagId
      (oas-summary): Delete a tag
      responses:
        '200':
          body:
            application/json:
              type: System.Object
        '401':
          description: Unauthorized
        '500':
          description: Internal Server Error
      (oas-tags):
        - Tags
    get:
      displayName: GET_tags-tagId
      (oas-summary): Retrieve a tag
      responses:
        '200':
          body:
            application/json:
              type: Api.Core.Dto.Tags.Tag
        '401':
          description: Unauthorized
        '500':
          description: Internal Server Error
      (oas-tags):
        - Tags
    /datapoints:
      displayName: datapoints
      delete:
        displayName: DELETE_tags-tagId-datapoints
        (oas-summary): Delete the association of this tag with all datapoints
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Responses.EntityUri_System.Int64_
          '401':
            description: Unauthorized
          '500':
            description: Internal Server Error
        (oas-tags):
          - Tags
      get:
        displayName: GET_tags-tagId-datapoints
        (oas-summary): List of all the datapoints associated to the user filtered by this tag
        responses:
          '200':
            body:
              application/json:
                type: Api.Core.Responses.EntitiesResponse_Api.Core.Responses.EntityUri_System.Int64__
          '401':
            description: Unauthorized
          '404':
            description: Not found
          '500':
