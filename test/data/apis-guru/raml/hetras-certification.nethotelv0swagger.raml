#%RAML 1.0
title: hetras Hotel API Version 0
version: v0
baseUri: 'https://api.hetras-certification.net/'
mediaType:
  - application/json
  - text/json
protocols:
  - HTTPS
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_hetras.zendesk.com_hc_logo.gif'
  (oas-x-origin):
    format: swagger
    url: 'https://developer.hetras.com/swagger/spec/HotelAPIv0.json'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): hetras-certification.net
  (oas-x-serviceName): hotel
annotationTypes:
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
  oas-definition-name:
    type: string
    allowedTargets: TypeDeclaration
/api:
  displayName: api
  /hotel:
    displayName: hotel
    /v0:
      displayName: v0
      /hotels:
        displayName: hotels
        '/{hotelId}':
          displayName: '{hotelId}'
          /rateplans:
            displayName: rateplans
            '/{rateplanCode}':
              displayName: '{rateplanCode}'
              /rates:
                displayName: rates
                /$count:
                  displayName: $count
                  get:
                    displayName: RatePlans_GetRatesCount
                    (oas-summary): Get the count of all active and loaded daily rates for the defined rateplan in a specified time period.
                    headers:
                      App-Id:
                        type: string
                        description: Application identifier
                        required: true
                      App-Key:
                        type: string
                        description: Application key.
                        required: true
                    responses:
                      '200':
                        body:
                          application/json:
                            type: TotalCountResponse
                        description: The count of daily rates for the specified time period.
                      '401':
                        description: 'You provided wrong credentials, or you reached your API limit.'
                      '403':
                        description: The application does not have access to the requested resource.
                      '404':
                        description: Not Found. The server has not found anything matching the Request-URI or the requested room could not be found.
                      '422':
                        description: The request failed to validate.
                      '500':
                        description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                      '503':
                        description: The server is currently unavailable. Please try later.
                    queryParameters:
                      from:
                        description: "Defines the last business day you would like to get rates for. The maximum time span between <i>from</i>´and <i>to</i>\r\n            is limited to 365 days."
                        format: rfc3339
                        required: true
                        type: datetime
                      to:
                        description: Defines the first business day you would like to get rates for.
                        format: rfc3339
                        required: true
                        type: datetime
                    (oas-tags):
                      - RatePlans
                '/{businessDay}':
                  displayName: '{businessDay}'
                  uriParameters:
                    businessDay:
                      description: The business day of the daily rate you want to update.
                      format: rfc3339
                      required: true
                      type: datetime
                  get:
                    displayName: RatePlans_GetRate
                    description: Read the setup of the daily rate for the defined rateplan for that specific business day.
                    (oas-summary): Get the setup of a daily rate for a specific business day and rateplan.
                    headers:
                      App-Id:
                        type: string
                        description: Application identifier
                        required: true
                      App-Key:
                        type: string
                        description: Application key.
                        required: true
                    responses:
                      '200':
                        body:
                          application/json:
                            type: RateResponse
                        description: Daily rate with room type supplements for the specified rateplan and business day.
                      '401':
                        description: 'You provided wrong credentials, or you reached your API limit.'
                      '403':
                        description: The application does not have access to the requested resource.
                      '404':
                        description: "Not Found. The server has not found anything matching the Request-URI or the requested\r\n            rateplan could not be found or the rate is inactive on this business day."
                      '422':
                        description: The request failed to validate.
                      '500':
                        description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                      '503':
                        description: The server is currently unavailable. Please try later.
                    (oas-tags):
                      - RatePlans
                  patch:
                    displayName: RatePlans_PatchRate
                    description: "The hetras API is using this <a href=\"https://developer.hetras.com/docs/patch\" onfocus=\"this.blur()\" target=\"_blank\">Patch Specification</a>\r\n            to partially update an existing resource. Currently this call only allows to set the base price for non-derived rateplans if the rateplan\r\n            is active and already loaded for the specified business day.\r\n            <br /><br />\r\n            A request example:<br /><pre>\r\n            [\r\n              {\r\n                \"op\": \"replace\", \"path\": \"/base_price\", \"value\": 120.00\r\n              }\r\n            ]\r\n            </pre><br />\r\n            For more details on how the API responds to errors please check our documentation on \r\n            <a href=\"https://developer.hetras.com/docs/errors/\" onfocus=\"this.blur()\">Error Handling</a>.<br />"
                    (oas-summary): Partially update a rate of the specified rateplan for a defined business day.
                    body:
                      application/json:
                        type: JsonPatchDocument_RatePatchRequest_
                        (oas-body-name): patchRequest
                        description: A set of JSON Patch operations.
                    headers:
                      App-Id:
                        type: string
                        description: Application identifier
                        required: true
                      App-Key:
                        type: string
                        description: Application key.
                        required: true
                    responses:
                      '200':
                        body:
                          application/json:
                            type: Object
                        description: The update was successful
                      '400':
                        description: Bad request. Request body erroneous.
                      '401':
                        description: 'You provided wrong credentials, or you reached your API limit.'
                      '403':
                        description: The application does not have access to the requested resource.
                      '404':
                        description: Not Found. The server has not found anything matching the Request-URI.
                      '422':
                        description: The request failed to validate.
                      '500':
                        description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                      '503':
                        description: The server is currently unavailable. Please try later.
                    (oas-tags):
                      - RatePlans
                patch:
                  displayName: RatePlans_PatchRates
                  description: "The hetras API is using this <a href=\"https://developer.hetras.com/docs/patch\" onfocus=\"this.blur()\" target=\"_blank\">Patch Specification</a>\r\n            to partially update an existing resource. Currently this call only allows to set the base price for non-derived rateplans if the rateplan\r\n            is active and already loaded for the specified time period.\r\n            <br /><br />\r\n            A request example:<br /><pre>\r\n            [\r\n              {\r\n                \"op\": \"replace\", \"path\": \"/base_price\", \"value\": 120.00\r\n              }\r\n            ]\r\n            </pre><br />\r\n            For more details on how the API responds to errors please check our documentation on \r\n            <a href=\"https://developer.hetras.com/docs/errors/\" onfocus=\"this.blur()\">Error Handling</a>.<br />"
                  (oas-summary): Partially update a rate of the specified rateplan for the defined time period.
                  body:
                    application/json:
                      type: JsonPatchDocument_RatePatchRequest_
                      (oas-body-name): patchRequest
                      description: A set of JSON Patch operations.
                  headers:
                    App-Id:
                      type: string
                      description: Application identifier
                      required: true
                    App-Key:
                      type: string
                      description: Application key.
                      required: true
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Object
                      description: The update was successful
                    '400':
                      description: Bad request. Request body erroneous.
                    '401':
                      description: 'You provided wrong credentials, or you reached your API limit.'
                    '403':
                      description: The application does not have access to the requested resource.
                    '404':
                      description: Not Found. The server has not found anything matching the Request-URI.
                    '422':
                      description: The request failed to validate.
                    '500':
                      description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                    '503':
                      description: The server is currently unavailable. Please try later.
                  queryParameters:
                    from:
                      description: "Defines the last business day you would like to get rates for. The maximum time span between <i>from</i>´and <i>to</i>\r\n            is limited to 365 days."
                      format: rfc3339
                      required: true
                      type: datetime
                    to:
                      description: Defines the first business day you would like to get rates for.
                      format: rfc3339
                      required: true
                      type: datetime
                  (oas-tags):
                    - RatePlans
                get:
                  displayName: RatePlans_GetRates
                  description: "With this call you can read the daily rates setup including the cancellation policy and minimum guarantee per day for the\r\n            specified rateplan. You can specify a timeperiod to read the daily rates for. The rateplan needs to be active for at least\r\n            one business day in the defined time period and have rates loaded."
                  (oas-summary): Get the setup of the daily rates for a specific rateplan and a defined timeperiod.
                  headers:
                    App-Id:
                      type: string
                      description: Application identifier
                      required: true
                    App-Key:
                      type: string
                      description: Application key.
                      required: true
                  responses:
                    '200':
                      body:
                        application/json:
                          type: RatesResponse
                      description: List of daily rates within the specified time preiod.
                    '204':
                      description: The rateplan does not have any active date or is not loaded within the defined time period.
                    '401':
                      description: 'You provided wrong credentials, or you reached your API limit.'
                    '403':
                      description: The application does not have access to the requested resource.
                    '404':
                      description: Not Found. The server has not found anything matching the Request-URI or the requested rateplan could not be found.
                    '422':
                      description: The request failed to validate.
                    '500':
                      description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                    '503':
                      description: The server is currently unavailable. Please try later.
                  queryParameters:
                    expand:
                      description: You can expand the supplements per room type on demand. By default they are not shown.
                      enum:
                        - RoomTypeSupplements
                      required: false
                      type: string
                    from:
                      description: "Defines the last business day you would like to get rates for. The maximum time span between <i>from</i>´and <i>to</i>\r\n            is limited to 365 days."
                      format: rfc3339
                      required: true
                      type: datetime
                    to:
                      description: Defines the first business day you would like to get rates for.
                      format: rfc3339
                      required: true
                      type: datetime
                    skip:
                      description: Amount of items to skip.
                      format: int32
                      required: false
                      type: integer
                    top:
                      description: Amount of items to select.
                      format: int32
                      required: false
                      type: integer
                    inlinecount:
                      description: Return total amount of items for a given filter criteria.
                      enum:
                        - None
                        - AllPages
                      required: false
                      type: string
                  (oas-tags):
                    - RatePlans
              uriParameters:
                rateplanCode:
                  description: The code of the rateplan you want to see details for.
                  required: true
                  type: string
              get:
                displayName: RatePlans_GetRateplan
                description: Read the details about a specific rateplan for the defined hotel.
                (oas-summary): Get all the details for a specific rateplan in the hotel.
                headers:
                  App-Id:
                    type: string
                    description: Application identifier
                    required: true
                  App-Key:
                    type: string
                    description: Application key.
                    required: true
                responses:
                  '200':
                    body:
                      application/json:
                        type: ExtendedRateplanEntry
                    description: Rateplan details for the given rateplan and hotel.
                  '401':
                    description: 'You provided wrong credentials, or you reached your API limit.'
                  '403':
                    description: The application does not have access to the requested resource.
                  '404':
                    description: Not Found. The server has not found anything matching the Request-URI or the requested rateplan could not be found.
                  '422':
                    description: The request failed to validate.
                  '500':
                    description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                  '503':
                    description: The server is currently unavailable. Please try later.
                (oas-tags):
                  - RatePlans
            get:
              displayName: RatePlans_GetRateplans
              description: "With this call you can find rateplans for a hotel by different filters. By default and without any filter criteria\r\n            defined it will return you all active rateplans."
              (oas-summary): Get a list of rateplans for the specified hotel id matching the filter criteria.
              headers:
                App-Id:
                  type: string
                  description: Application identifier
                  required: true
                App-Key:
                  type: string
                  description: Application key.
                  required: true
              responses:
                '200':
                  body:
                    application/json:
                      type: RateplansListResponse
                  description: A list of rateplans for a requested hotel matching the filter criteria.
                '204':
                  description: There are no rateplans found for the given filtering and pagination criteria.
                '401':
                  description: 'You provided wrong credentials, or you reached your API limit.'
                '403':
                  description: The application does not have access to the requested resource.
                '404':
                  description: Not Found. The server has not found anything matching the Request-URI.
                '422':
                  description: The request failed to validate.
                '500':
                  description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                '503':
                  description: The server is currently unavailable. Please try later.
              queryParameters:
                sellingStatus:
                  description: "Specify which rateplans to return. If you do not specify a value you will by default get active\r\n            rateplans."
                  enum:
                    - Active
                    - Inactive
                    - All
                  required: false
                  type: string
                commissionable:
                  description: Return all rateplans having commisionable status
                  required: false
                  type: boolean
                group:
                  description: Return all rateplans belonging to the specified rateplan group
                  required: false
                  type: string
                baseRateplan:
                  description: Return all rateplans having the specified rateplan as base rateplan
                  required: false
                  type: string
                channelGroup:
                  description: Return all rateplans that are sold through at least one channel out of the specified channel group
                  required: false
                  type: string
                channelCode:
                  description: Return all rateplans sold through the specified channel
                  required: false
                  type: string
                marketCodes:
                  collectionFormat: csv
                  description: Return all rateplans having one of the specified values as a market code
                  items:
                    type: string
                  required: false
                  type: array
                  facets:
                    collectionFormat: string
                roomTypes:
                  collectionFormat: csv
                  description: Return all rateplans by which at least one of the specified room types are sold
                  items:
                    type: string
                  required: false
                  type: array
                  facets:
                    collectionFormat: string
                includedServices:
                  collectionFormat: csv
                  description: Return all rateplans having at least one of the specified services included
                  items:
                    type: string
                  required: false
                  type: array
                  facets:
                    collectionFormat: string
                skip:
                  description: Amount of items to skip.
                  format: int32
                  required: false
                  type: integer
                top:
                  description: Amount of items to select.
                  format: int32
                  required: false
                  type: integer
                inlinecount:
                  description: Return total amount of items for a given filter criteria.
                  enum:
                    - None
                    - AllPages
                  required: false
                  type: string
              (oas-tags):
                - RatePlans
            /$count:
              displayName: $count
              get:
                displayName: RatePlans_GetRateplansCount
                (oas-summary): Get the count of all rateplans in the hotel matching the given filter criteria.
                headers:
                  App-Id:
                    type: string
                    description: Application identifier
                    required: true
                  App-Key:
                    type: string
                    description: Application key.
                    required: true
                responses:
                  '200':
                    body:
                      application/json:
                        type: TotalCountResponse
                    description: The rateplans count in the hotel matching the given filter criteria.
                  '401':
                    description: 'You provided wrong credentials, or you reached your API limit.'
                  '403':
                    description: The application does not have access to the requested resource.
                  '404':
                    description: Not Found. The server has not found anything matching the Request-URI.
                  '422':
                    description: The request failed to validate.
                  '500':
                    description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                  '503':
                    description: The server is currently unavailable. Please try later.
                queryParameters:
                  sellingStatus:
                    description: "Specify which rateplans to return. If you do not specify a value you will by default get active\r\n            rateplans."
                    enum:
                      - Active
                      - Inactive
                      - All
                    required: false
                    type: string
                  commissionable:
                    description: Return all rateplans having commisionable status
                    required: false
                    type: boolean
                  group:
                    description: Return all rateplans belonging to the specified rateplan group
                    required: false
                    type: string
                  baseRateplan:
                    description: Return all rateplans having the specified rateplan as base rateplan
                    required: false
                    type: string
                  channelGroup:
                    description: Return all rateplans that are sold through at least one channel out of the specified channel group
                    required: false
                    type: string
                  channelCode:
                    description: Return all rateplans sold through the specified channel
                    required: false
                    type: string
                  marketCodes:
                    collectionFormat: csv
                    description: Return all rateplans having one of the specified values as a market code
                    items:
                      type: string
                    required: false
                    type: array
                    facets:
                      collectionFormat: string
                  roomTypes:
                    collectionFormat: csv
                    description: Return all rateplans by which at least one of the specified room types are sold
                    items:
                      type: string
                    required: false
                    type: array
                    facets:
                      collectionFormat: string
                  includedServices:
                    collectionFormat: csv
                    description: Return all rateplans having at least one of the specified services included
                    items:
                      type: string
                    required: false
                    type: array
                    facets:
                      collectionFormat: string
                (oas-tags):
                  - RatePlans
          /rooms:
            displayName: rooms
            '/{roomNumber}':
              displayName: '{roomNumber}'
              uriParameters:
                roomNumber:
                  description: The room number you want to see details for.
                  required: true
                  type: string
              patch:
                displayName: Rooms_PatchRoom
                description: "The hetras API is using this <a href=\"https://developer.hetras.com/docs/patch\" onfocus=\"this.blur()\" target=\"_blank\">Patch Specification</a>\r\n            to partially update an existing resource. Currently this call only allows to modify condition and housekeeping occupancy status of the room.\r\n            <br /><br />\r\n            A request example:<br /><pre>\r\n            [\r\n              {\r\n                \"op\": \"replace\", \"path\": \"/status/condition\", \"value\": \"CleanNotInspected\"\r\n              }, {\r\n                \"op\": \"replace\", \"path\": \"/status/housekeeping_occupancy\", \"value\": \"Vacant\"\r\n              }\r\n            ]\r\n            </pre><br />\r\n            For more details on how the API responds to errors please check our documentation on \r\n            <a href=\"https://developer.hetras.com/docs/errors/\" onfocus=\"this.blur()\">Error Handling</a>.<br />"
                (oas-summary): Partially updates a room.
                body:
                  application/json:
                    type: JsonPatchDocument_RoomPatchRequest_
                    (oas-body-name): patchRequest
                    description: A set of JSON Patch operations.
                headers:
                  App-Id:
                    type: string
                    description: Application identifier
                    required: true
                  App-Key:
                    type: string
                    description: Application key.
                    required: true
                responses:
                  '200':
                    body:
                      application/json:
                        type: Object
                    description: The update was successful
                  '400':
                    description: Bad request. Request body erroneous.
                  '401':
                    description: 'You provided wrong credentials, or you reached your API limit.'
                  '403':
                    description: The application does not have access to the requested resource.
                  '404':
                    description: Not Found. The server has not found anything matching the Request-URI.
                  '422':
                    description: The request failed to validate.
                  '500':
                    description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                  '503':
                    description: The server is currently unavailable. Please try later.
                (oas-tags):
                  - Rooms
              get:
                displayName: Rooms_GetRoom
                description: With this call you can load the details about a specific room in the hotel. It will show you the current status of the room.
                (oas-summary): Get all the details for a specific room in the hotel.
                headers:
                  App-Id:
                    type: string
                    description: Application identifier
                    required: true
                  App-Key:
                    type: string
                    description: Application key.
                    required: true
                responses:
                  '200':
                    body:
                      application/json:
                        type: Room
                    description: Room details for the given room and hotel.
                  '401':
                    description: 'You provided wrong credentials, or you reached your API limit.'
                  '403':
                    description: The application does not have access to the requested resource.
                  '404':
                    description: Not Found. The server has not found anything matching the Request-URI or the requested room could not be found.
                  '500':
                    description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                  '503':
                    description: The server is currently unavailable. Please try later.
                (oas-tags):
                  - Rooms
            /available:
              displayName: available
              get:
                displayName: Rooms_GetAvailableRooms
                (oas-summary): Request available rooms using a given criteria.
                headers:
                  App-Id:
                    type: string
                    description: Application identifier
                    required: true
                  App-Key:
                    type: string
                    description: Application key.
                    required: true
                responses:
                  '200':
                    body:
                      application/json:
                        type: RoomListResponse
                    description: A list of available rooms in the specified time period.
                  '204':
                    description: There are no available rooms found for the given criteria.
                  '401':
                    description: 'You provided wrong credentials, or you reached your API limit.'
                  '403':
                    description: The application does not have access to the requested resource.
                  '404':
                    description: Not Found. The server has not found anything matching the Request-URI or the requested room could not be found.
                  '422':
                    description: The request failed to validate.
                  '500':
                    description: We caught an unexpected error on our side. Please report with providing the Hetras-Tracking-Id from the response headers and we will check the logfiles.
                  '503':
                    description: The server is currently unavailable. Please try later.
                queryParameters:
                  from:
                    description: "Rooms returned will not be assigned to a reservation or be under maintenance between this date\r\n            and the specified to date. Still there could be departing reservations or ending maintenances\r\n            for this date."
                    format: rfc3339
                    required: true
                    type: datetime
                  to:
                    description: "Rooms returned will not be assigned to a reservation or be under maintenance between the specified\r\n            from date and this date. Still there could be arriving reservations or beginning maintenances\r\n            for this date."
                    format: rfc3339
                    required: true
                    type: datetime
                  adults:
                    description: Specifies number of adults the returned rooms will have to be able to house. The default value is 1.
                    format: byte
                    required: false
                    type: string
                    facets:
                      format: string
                  includeOutOfService:
                    description: "Should rooms that are set OutOfService in the defined time period be returned as available. By default\r\n            they are not."
                    required: false
                    type: boolean
                  roomTypes:
                    collectionFormat: csv
                    description: Return result only for rooms for the given room types. Allows to pass a comma-separated list of room types.
                    items:
                      type: string
                    required: false
                    type: array
                    facets:
                      collectionFormat: string
                  amenities:
                    collectionFormat: csv
                    description: "Return result only for rooms having all of the given amenities. You can provide a comma seperated list of \r\n            amenity codes."
                    items:
                      type: string
                    required: false
                    type: array
                    facets:
                      collectionFormat: string
                  views:
                    collectionFormat: csv
                    description: "Return result only for rooms having at least one of the specified views. You can provide a comma seperated list of \r\n            view codes."
                    items:
                      type: string
                    required: false
                    type: array
                    facets:
                      collectionFormat: string
                  locations:
                    collectionFormat: csv
                    description: "Return result only for rooms having at least one of the specified locations. You can provide a comma seperated list of \r\n            location codes."
                    items:
                      type: string
                    required: false
                    type: array
                    facets:
                      collectionFormat: string
                  skip:
                    description: Amount of items to skip.
                    format: int32
                    required: false
                    type: integer
                  top:
                    description: Amount of items to select.
                    format: int32
                    required: false
                    type: integer
                  inlinecount:
                    description: Return total amount of items for a given filter criteria.
                    enum:
                      - None
                      - AllPages
                    required: false
                    type: string
                (oas-tags):
                  - Rooms
            /$count:
              displayName: $count
              get:
                displayName: Rooms_GetRoomsCount
                (oas-summary): Get the count of all rooms in the hotel matching the given filter criteria.
                headers:
                  App-Id:
                    type: string
                    description: Application identifier
                    required: true
                  App-Key:
                    type: string
                    description: Application key.
                    required: true
                responses:
                  '200':
                    body:
                      application/json:
                        type: TotalCountResponse
                    description: The room count in the hotel matching the given filter criteria.
                  '401':
                    description: 'You provided wrong credentials, or you reached your API limit.'
                  '403':
                    description: The application does not have access to the requested resource.
                  '404':
                    description: Not Found. The server has not found anything matching the Request-URI or the requested room could not be found.
