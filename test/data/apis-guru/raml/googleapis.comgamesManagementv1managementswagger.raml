/applications:
  displayName: applications
  '/{applicationId}':
    displayName: '{applicationId}'
    /players:
      displayName: players
      /hidden:
        displayName: hidden
        '/{playerId}':
          displayName: '{playerId}'
          uriParameters:
            playerId:
              description: A player ID. A value of me may be used in place of the authenticated player's ID.
              required: true
              type: string
              displayName: A player ID. A value of me may be used in place of the authenticated player's ID.
          delete:
            displayName: gamesManagement.players.unhide
            description: Unhide the given player's leaderboard scores from the given application. This method is only available to user accounts for your developer console.
            responses:
              '200':
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/plus.login'
            (oas-tags):
              - players
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
          post:
            displayName: gamesManagement.players.hide
            description: Hide the given player's leaderboard scores from the given application. This method is only available to user accounts for your developer console.
            responses:
              '200':
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/plus.login'
            (oas-tags):
              - players
        get:
          displayName: gamesManagement.applications.listHidden
          description: Get the list of players hidden from the given application. This method is only available to user accounts for your developer console.
          responses:
            '200':
              body:
                application/json:
                  type: HiddenPlayerList
              description: Successful response
          queryParameters:
            maxResults:
              description: 'The maximum number of player resources to return in the response, used for paging. For any response, the actual number of player resources returned may be less than the specified maxResults.'
              maximum: 50
              minimum: 1
              type: integer
              displayName: 'The maximum number of player resources to return in the response, used for paging. For any response, the actual number of player resources returned may be less than the specified maxResults.'
            pageToken:
              description: The token returned by the previous request.
              type: string
              displayName: The token returned by the previous request.
          securedBy:
